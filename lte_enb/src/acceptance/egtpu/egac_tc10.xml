#
#       Test:    10.33
#
#       Desc:   Verify the successful exchange of Command Message
#               b/w MME and SGW with same seq no
#
#       Ref:   None
#
#       Notes:  None
#              
#       Module: TPT Module
#
#       File:   egac_tc7.xml
#
<tapa.testcase id="10.33">
   
   # Document subsection
   <tapa.doc>

      <tapa.rfc id="None" section="None"/>
      <tapa.module id="eg.tpt"/>

      <tapa.desc>
         "Verify the successful exchange of Command Message b/w MME and SGW"
      </tapa.desc>

      <tapa.longdesc/>
       
   </tapa.doc>
   
   # Require features and flags subsection 
   <tapa.require>

      <tapa.ifdef>
         <tapa.flag id="EGTP_C"/>
      </tapa.ifdef>
         
      <tapa.ifndef/>
        
      <tapa.mode mode="single"/>

   </tapa.require>
     
   # Configuration subsection
   <tapa.cfg>

    # Configure TUCL
      <tapa.hi.init/>

    # Configure EGTP as SGW and PGW
      <tapa.eg.mme1.sgw1.init/>

   </tapa.cfg>
   
   # Test Steps subsection
   <tapa.test>

      <tapa.send>
         <tapa.egt.sigreq egtpMsgType="EGT_GTPC_MSG_CS_REQ" intfType="EGT_GTP_INT_S11" 
             crtFlag="TRUE" spId="1" procId="200" transId="1" addInterfaceType="10"
               lclPort="3123" rmtPort="2124" t3TmrVal="10" lcladdr="127.0.0.1" 
               rmtaddr="127.0.0.1"/>
      </tapa.send>
       
      <tapa.expect>
         <tapa.egt.sigind teid="1" suId="2" procId="300" transId="1"/>   
      </tapa.expect>
   
      <tapa.send>
         <tapa.egt.sigrsp egtpMsgType="EGT_GTPC_MSG_CS_RSP" intfType="EGT_GTP_INT_S11" 
             teid="1" spId="1" procId="300" addInterfaceType="11" teidValue="1" 
             transId="1"/>
      </tapa.send>
       
      <tapa.expect>
         <tapa.egt.sigcfm egtpMsgType="EGT_GTPC_MSG_CS_RSP" transId="1" teid="1" 
            suId="1" procId="200"/>
      </tapa.expect>
  
      <tapa.send>
        <tapa.egt.sigreq egtpMsgType="EGT_GTPC_MSG_MB_CMND" intfType="EGT_GTP_INT_S11" 
            crtFlag="FALSE" seqNoPres="TRUE" seqNo="8388609" spId="1" procId="200" transId="1" 
            addInterfaceType="10" lclPort="3123" rmtPort="2124" t3TmrVal="100" 
            lcladdr="127.0.0.1" rmtaddr="127.0.0.1"/>
      </tapa.send>
      
      <tapa.expect>
         <tapa.egt.sigind teid="1" suId="2" procId="300" transId="1"/>   
      </tapa.expect>
       <tapa.send>
        <tapa.egt.sigreq egtpMsgType="EGT_GTPC_MSG_MB_CMND" intfType="EGT_GTP_INT_S11" 
            crtFlag="FALSE" seqNoPres="TRUE" seqNo="8388609" spId="1" procId="200" transId="1" 
            addInterfaceType="10" lclPort="3123" rmtPort="2124" t3TmrVal="100" 
            lcladdr="127.0.0.1" rmtaddr="127.0.0.1"/>
      </tapa.send>
      
      <tapa.expect>
         <tapa.egt.errind teid="1" suId="1" procId="200" transId="1"/>   
      </tapa.expect>
   
 
      
   </tapa.test>
   
   # Clean subsection
   <tapa.clean/>
         
   # Shutdown subsection
   
   <tapa.shutdown>

   # De-Init EGTP
   <tapa.egtp.deinit/>

   
   # De-Init TUCL
   <tapa.hi.deinit/>

   </tapa.shutdown>
   
</tapa.testcase>


