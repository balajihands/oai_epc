   <tapa.doc>

      <tapa.rfc id="None" section="None"/>
      <tapa.module id="eg.edm"/>

      <tapa.desc>
	      "Test the sending of Supported extension header
			 notification message with missing mandatory IE

      </tapa.desc>

      <tapa.longdesc/>

   </tapa.doc>

   # Require features and flags subsection
   <tapa.require>

      <tapa.ifdef>
         <tapa.flag id="EGTP_U"/>
      <tapa.flag id="EGTP_U_REORDER_ENABLE"/>
      </tapa.ifdef>	

      <tapa.ifndef/>

      <tapa.mode mode="single"/>

   </tapa.require>

   # Configuration subsection
   <tapa.cfg>
   # Configure TUCL	   
      <!--      
      <tapa.hi.init/>
       -->
   # Configure GTP-U   
      <tapa.eg.gtpu.init/>

   </tapa.cfg>

   # Test Steps subsection
   <tapa.test>

      # Send Supported Extension Header Notification  message
      <tapa.send>
           <tapa.egt.egudatreq pduNmbPres="0" npduNmb="0"
           seqNoPres="TRUE" seqNo="1" udpextPres="0" udpPortNmb="0"
           pdcpextPres="1" pdcpPortNmb="0" noIEs="TRUE"
			  msgType="EGT_GTPU_MSG_SUPP_EXT_HDR_NTF"
           remTeid="0" lclTeid="0"/>
      </tapa.send>

     <tapa.expect >
         <tapa.egt.eguerrind suId="0" lclTeid="0" 
			errCause="EGT_ERR_MISSING_MAND_IE"
			failedMsgType="EGT_GTPU_MSG_SUPP_EXT_HDR_NTF"
			lclAddr="127.0.0.1" rmtAddr="127.0.0.1" failedIeType="EGU_IE_EXT_HDR_LST"/>
      </tapa.expect>

      <tapa.delay delay="20" />
   </tapa.test>
   # Clean subsection
   <tapa.clean/>

   # Shutdown subsection

   <tapa.shutdown>
   # De-Init EGTP
   <tapa.egtpu.deinit/>

   # De-Init TUCL
   <!--
   <tapa.hi.deinit/>
   -->
   </tapa.shutdown>
</tapa.testcase>

