#
#       Test:    6.38
#
#       Desc:   Verify the MBMS Session Delete request and response on SM interface
#
#       Ref:   None
#
#       Notes:  None
#              
#       Module: TPT Module
#
#       File:   egac_tc6.xml
#
<tapa.testcase id="6.38">
   
   # Document subsection
   <tapa.doc>

      <tapa.rfc id="None" section="None"/>
      <tapa.module id="eg.tpt"/>

      <tapa.desc>
         "Verify the sending of MBMS Session Start and Stop Request"
      </tapa.desc>

      <tapa.longdesc/>
       
   </tapa.doc>
   
   # Require features and flags subsection 
   <tapa.require>

      <tapa.ifdef>
         <tapa.flag id="EGTP_C"/>
      </tapa.ifdef>
         
      <tapa.ifndef/>
        
      <tapa.mode mode="single"/>

   </tapa.require>
     # Configuration subsection
   <tapa.cfg>

    # Configure TUCL
      <tapa.hi.init/>
 
      <tapa.eg.mme10.mbmsgw1.init/>
   </tapa.cfg>
   
   # Test Steps subsection
   <tapa.test>

	<tapa.send>
	<tapa.egt.sigreq egtpMsgType="EGT_GTPC_MSG_MBMS_SESS_START_REQ" intfType="EGT_GTP_INT_SM" crtFlag="TRUE" 
	spId="1" procId="200" transId="1" addInterfaceType="26" FTeidIntfType="EGT_INT_SM_MBMSGW_GTP_C"
	lclPort="3129" rmtPort="2123" t3TmrVal="10" lcladdr="127.0.0.1" rmtaddr="127.0.0.1"/>
	</tapa.send>

	<tapa.expect>
	<tapa.egt.sigind teid="1" suId="2" procId="300" transId="1"/>   
	</tapa.expect>

	<tapa.send>
	<tapa.egt.sigrsp egtpMsgType="EGT_GTPC_MSG_MBMS_SESS_START_RSP" intfType="EGT_GTP_INT_SM" teid="1" 
	spId="1" procId="300" addInterfaceType="24" teidValue="1" transId="1" FTeidIntfType="EGT_INT_SM_MME_GTP_C"/>
	</tapa.send>

	<tapa.expect>
	<tapa.egt.sigcfm egtpMsgType="EGT_GTPC_MSG_MBMS_SESS_START_RSP" transId="1" teid="1" suId="1" procId="200"/>
	</tapa.expect>

	<tapa.send>
	<tapa.egt.sigreq egtpMsgType="EGT_GTPC_MSG_MBMS_SESS_STOP_REQ" intfType="EGT_GTP_INT_SM" teid="1"
	spId="1" procId="200" lclPort="3129" rmtPort="2123" t3TmrVal="10" seqNo="1"
	lcladdr="127.0.0.1" rmtaddr="127.0.0.1" teidPres="PRSNT_NODEF" teidValue="1"/>
	</tapa.send>

# at MME this should not create a new tunnel since it is sent to existing TEID
	<tapa.expect>
	<tapa.egt.sigind egtpMsgType="EGT_GTPC_MSG_MBMS_SESS_STOP_REQ" teid="1" suId="2" procId="300"/>
	</tapa.expect>

# Send response from MME 
	<tapa.send>
	<tapa.egt.sigrsp egtpMsgType="EGT_GTPC_MSG_MBMS_SESS_STOP_RSP" teid="1" seqNo="1" intfType="EGT_GTP_INT_SM" spId="1" procId="300" teidValue="1"/>
	</tapa.send>

# receive response at MME
	<tapa.expect>
	<tapa.egt.sigcfm egtpMsgType="EGT_GTPC_MSG_MBMS_SESS_STOP_REQ" teid="1" suId="1" procId="200"/>
	</tapa.expect>

#delete tunnel 1 at MBMSGW
	<tapa.send>
	<tapa.egt.lcldelreq teid="1" procId="200" spId="1"/>
	</tapa.send>

	<tapa.expect>
	<tapa.egt.lcldelcfm teid="1" suId="1" procId="200"/>
	</tapa.expect>

# Delete Tunnel at MME
	<tapa.send>
	<tapa.egt.lcldelreq teid="1" procId="300" spId="1"/>
	</tapa.send>

	<tapa.expect>
	<tapa.egt.lcldelcfm teid="1" suId="2" procId="300"/>
	</tapa.expect>

   </tapa.test>

   # Clean subsection
   <tapa.clean/>
         
   # Shutdown subsection
   
   <tapa.shutdown>

   # De-Init EGTP
   <tapa.egtp.deinit/>

   
   # De-Init TUCL
   <tapa.hi.deinit/>

   </tapa.shutdown>
   
</tapa.testcase>


