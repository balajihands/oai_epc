      <tapa.send>
         <tapa.tfu.tti.ind numTti="7"/>
      </tapa.send>

      <tapa.expect>
         # Expect staInd from MAC to RLC for SPS service for SPS ocassion  
         <tapa.rgu.dstaind lchTb1Data="3"  saveTo="3"/>
      </tapa.expect>

