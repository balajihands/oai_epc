   </tapa.require>

   # Configuration subsection
   <tapa.cfg>

      # Initialize LTEMAC
      <tapa.rg.macInit/>

      <tapa.rg.dbgDeInitAll/>
      #Initialize CELL
      <tapa.rg.cellInit  DLSPSMaxBw = "50"  ulTotalBw="100"  numTxAntPorts="2" startRnti="70"
      rntiSize="11" isDlFreqSel="1" hopOffst="20"/>
