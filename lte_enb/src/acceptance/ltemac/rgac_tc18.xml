   </tapa.cfg>

   # Test Steps subsection
   <tapa.test>
     
      <tapa.tfu.directive ulDeltaPrsnt="TRUE"/> 
      # Optimally set UL CQI so that all the UEs get scheduled using 1 subband only.
      <tapa.send>
         <tapa.tfu.ulcqi.ind>
            <tapa.tfu.ulcqi ueId="0" isWideBand="TRUE" wideCqi="12"/>
         </tapa.tfu.ulcqi.ind>
      </tapa.send>
