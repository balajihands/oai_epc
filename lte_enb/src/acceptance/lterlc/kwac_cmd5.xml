#
#
#       Command: tapa.ckw.amcfg
#
#       Desc:    Command to configure AM Mode entity 
#
#       Notes:   None
#
#       Module:  RLC
#
#       File:    kwac_cmd.xml
#
#
<tapa.command id="tapa.ckw.amcfg">

   # Document subsection
   <tapa.doc>

      <tapa.desc>
         "Configure the RLC AM mode entity."
      </tapa.desc>

      <tapa.longdesc/>

   </tapa.doc>

   # Test Steps subsection
   <tapa.steps>

      ########################################################
      #           CONFIGURATION START
      ########################################################
      # Send TM mode Configuration Request for DCCH DCCH
      <tapa.send>
         <tapa.ckw.cfgreq  ueId="1" cellId="1" numEnt="2"
            cfgType="1:1"
            rbId="1:2"
            rbType="1:1"
            entMode="3:3"
            dir="3:3"
            logChId0Id="1:2"
#Pradeep
#            logChId0Type="5:5"
            logChId0Type="4:4"
            logChId1Id="1:2"
            logChId1Type="5:5"
            umDlSnLen="0:0"
            umUlSnLen="0:0"
            umUlReOrdTmr="0:0"
            amDlPollRetxTmr="500:500"
            amDlPollPdu="1:1"
            amDlPollByte="25000:50000"
            amDlMaxRetx="1:1"
            amUlStaProhTmr="0:0"
            amUlReOrdTmr="200:200"
         />
      </tapa.send>

      <tapa.delay delay="2"/>

      # Expect General Configuration Confirm
      <tapa.expect>
         <tapa.ckw.cfgcfm ueId="1" cellId="1" numEnt="2" 
            rbId="1:2" 
            rbType="1:1"
            status="1:1" reason="0:0"/>
      </tapa.expect>


      <tapa.send>
         <tapa.ckw.cfgreq  ueId="2" cellId="1" numEnt="2"
            cfgType="1:1"
            rbId="1:2"
            rbType="1:1"
            entMode="3:3"
            dir="3:3"
            logChId0Id="1:2"
#Pradeep
#            logChId0Type="5:5"
            logChId0Type="4:4"
            logChId1Id="1:2"
            logChId1Type="5:5"
            umDlSnLen="0:0"
            umUlSnLen="0:0"
            umUlReOrdTmr="0:0"
            amDlPollRetxTmr="500:500"
            amDlPollPdu="1:1"
            amDlPollByte="25000:50000"
            amDlMaxRetx="1:1"
            amUlStaProhTmr="0:0"
            amUlReOrdTmr="200:200"
         />
      </tapa.send>
      
      <tapa.delay delay="2"/>
      
      # Expect General Configuration Confirm
      <tapa.expect>
         <tapa.ckw.cfgcfm ueId="2" cellId="1" numEnt="2" 
            rbId="1:2" 
            rbType="1:1"
            status="1:1" reason="0:0"/>
      </tapa.expect>
      # Map the RB entities for UL and DL
      <tapa.send>
         <tapa.ckw.mapdlul ueId="1" cellId="1"
            numRb="2"
            dir="3:3"
            rbId="1:2"
            rbType="1:1"
            mode="3:3"
            othRbId="1:2" othUeId="2:2" othCellId="1:1"
            othRbType="1:1"
            dlLcId="1:2" dlLcType="4:4"
            ulLcId="1:2" ulLcType="1:2" />
      </tapa.send>


      <tapa.send>
         <tapa.ckw.mapdlul ueId="2" cellId="1"
            numRb="2"
            dir="3:3"
            rbId="1:2"
            rbType="1:1"
            mode="3:3"
            othRbId="1:2" othUeId="1:1" othCellId="1:1"
            othRbType="1:1"
            dlLcId="1:2" dlLcType="4:4"
            ulLcId="1:2" ulLcType="1:2" />
      </tapa.send>
      ########################################################
      #                 CONFIGURATION END
      ########################################################

   </tapa.steps>

</tapa.command>

