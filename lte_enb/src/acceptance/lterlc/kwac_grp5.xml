#
#
#       Test:    5.29
#
#       Desc:    PDU discard by corrupting reserved bits and sequence number 
#
#       Notes:   None
#              
#       Module:  rlc 
#
#       File:    kwac_grp5.xml
#
#
<tapa.testcase id="5.29">

   # Document subsection
   <tapa.doc>

      <tapa.rfc id="None" section="None"/>
      <tapa.module id="rlc"/>

      <tapa.desc>
         "sending PDU with invalid PDUID"
      </tapa.desc>

      <tapa.longdesc>
          "Acceptance test case No: 5.29
           1. Configure RLC through LM.
           2. Configure UM RLC entities with SN 10 bits.
           3. Tx PDCP send 9 SDUs size of 100 to Tx RLC.
           4. Tx RLC sends 3 DDatReq to MAC with PDUs of size 300.
           5. MAC sends 2 (2, 3) DDatInd to Rx RLC with PDU Header Corruption.
           6. Rx RLC should discard PDUs."
      </tapa.longdesc>
         
   </tapa.doc>

   # Require features and flags subsection 
   <tapa.require>
      
      <tapa.ifdef/>
        
      <tapa.ifndef/>

      <tapa.mode mode="single"/>

   </tapa.require>

   # Configuration subsection
   <tapa.cfg>
      <tapa.kw.init/>
      <tapa.kw.umcfg1/>
   </tapa.cfg>

   # Test Steps subsection
   <tapa.test>
      <tapa.repeat.start repeatCount="9"/>
         <tapa.send>
            <tapa.kwu.datreq entity="ENTPJ" sduId="1" sduSize="100" rbId="1"
               rbType="1" ueId="1" cellId="1" />
         </tapa.send>

         <tapa.expect>
            <tapa.dummy.rgu.dstarsp cellId="1" ueId="1" lcId="1" valdType="1"/>
         </tapa.expect>

      <tapa.repeat.end/>

      <tapa.send>
         <tapa.dummy.rgu.dstaind totBufSize="309" lcId="1" ueId="1" cellId="1" 
               numLch="1" transId="1"/>
      </tapa.send>

      #corrupt reserved bits
      <tapa.expect>
         <tapa.dummy.rgu.ddatreq numLch="1" ueId="1" cellId="1" lcId="1" transId="1"
               lchData="600:13:0:1:1:304:
                        0:0:1:0:0"/>
      </tapa.expect>

      <tapa.send>
         <tapa.dummy.rgu.dstaind totBufSize="309" lcId="1" ueId="1" cellId="1" 
               numLch="1" transId="2"/>
      </tapa.send>

      #corrupt reserved bits
      <tapa.expect>
         <tapa.dummy.rgu.ddatreq numLch="1" ueId="1" cellId="1" lcId="1" transId="2"
               lchData="300:7:0:1:1:304:
                        1:0:1:0:0"/>
      </tapa.expect>

      <tapa.send>
         <tapa.dummy.rgu.dstaind totBufSize="309" lcId="1" ueId="1" cellId="1" 
               numLch="1" transId="3"/>
      </tapa.send>

      #corrupt reserved bits
      <tapa.expect>
         <tapa.dummy.rgu.ddatreq numLch="1" ueId="1" cellId="1" lcId="1" transId="3"
               lchData="0:0:0:1:1:304:
                        2:0:1:0:0"/>
      </tapa.expect>

      <tapa.send>
         <tapa.dummy.rgu.ddatind numLch="1" cellId="1"  ueId="2" transId="2"
               lchData="1:1:0:
                        2:-1"  corruptHdr="TRUE"/>
      </tapa.send>


       #send the higher sequnce number hence corrupting PDU 
      <tapa.send>
         <tapa.dummy.rgu.ddatind numLch="1" cellId="1"  ueId="2" transId="3"
               lchData="1:1:0:
                        4:-1"  corruptHdr="TRUE"/>
      </tapa.send>

      #kw005.201 Removed delay of 5
   <tapa.delay delay="1"/> 

   </tapa.test>

   # Clean subsection
   <tapa.clean/>

   # Shutdown subsectitn
   <tapa.shutdown>
   <tapa.kw.deinit/>

   <tapa.delay delay="5"/> 

   </tapa.shutdown>

</tapa.testcase>


