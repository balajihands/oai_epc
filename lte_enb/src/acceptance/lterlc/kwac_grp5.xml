<tapa.testcase id="5.32">

   # Document subsection
   <tapa.doc>

      <tapa.rfc id="None" section="None"/>
      <tapa.module id="rlc"/>

      <tapa.desc>
         "Verification of Concatenation procedure"
      </tapa.desc>

      <tapa.longdesc>
          "Acceptance test case No: 5.32
           1. Configure RLC through LM
           2. Configure RLC entities
           3. PDCP sends 5 SDU'S with size 200 each
           4. MAC notifies the StaInd() with BO ()
           5. MAC receives only one PDU "
           6. MAC sends PDU to RLC(Rx)"
           7. RLC(Rx) indicates PDU to PDCP(Rx)"
      </tapa.longdesc>
         
   </tapa.doc>

   # Require features and flags subsection 
   <tapa.require>

      <tapa.ifdef/>
        
      <tapa.ifndef/>
         
      <tapa.mode mode="single"/>

   </tapa.require>

   # Configuration subsection
    <tapa.cfg>
      <tapa.kw.init/>
      <tapa.kw.umcfg1/>
   </tapa.cfg>
     
   # Test Steps subsection
   <tapa.test>
      #PDCP sends data request with SDU size 200
      <tapa.send>
         <tapa.kwu.datreq entity="ENTPJ" sduId="1" sduSize="200" rbId="1"
            rbType="1" ueId="1" cellId="1"/>
      </tapa.send>
      
      <tapa.expect>
         <tapa.dummy.rgu.dstarsp cellId="1" ueId="1" lcId="1" 
               bo="200" estHdrSz="3" staPduPrsnt="0"/>
      </tapa.expect>

      <tapa.send>
         <tapa.kwu.datreq entity="ENTPJ" sduId="2" sduSize="200" rbId="1"
            rbType="1" ueId="1" cellId="1"/>
      </tapa.send>

      <tapa.expect>
         <tapa.dummy.rgu.dstarsp cellId="1" ueId="1" lcId="1" 
               bo="400" estHdrSz="5" staPduPrsnt="0"/>
      </tapa.expect>

      <tapa.send>
         <tapa.kwu.datreq entity="ENTPJ" sduId="3" sduSize="200" rbId="1"
            rbType="1" ueId="1" cellId="1"/>
      </tapa.send>

      #MAC expects the Status Response from RLC
      <tapa.expect>
         <tapa.dummy.rgu.dstarsp cellId="1" ueId="1" lcId="1" 
               bo="600" estHdrSz="7" staPduPrsnt="0"/>
      </tapa.expect>
     
      <tapa.send>
         <tapa.kwu.datreq entity="ENTPJ" sduId="4" sduSize="200" rbId="1"
            rbType="1" ueId="1" cellId="1"/>
      </tapa.send>

      #MAC expects the Status Response from RLC
      <tapa.expect>
         <tapa.dummy.rgu.dstarsp cellId="1" ueId="1" lcId="1" 
               bo="800" estHdrSz="9" staPduPrsnt="0"/>
      </tapa.expect>

      <tapa.send>
         <tapa.kwu.datreq entity="ENTPJ" sduId="5" sduSize="200" rbId="1"
            rbType="1" ueId="1" cellId="1"/>
      </tapa.send>

      #MAC expects the Status Response from RLC
      <tapa.expect>
         <tapa.dummy.rgu.dstarsp cellId="1" ueId="1" lcId="1" 
               bo="1000" estHdrSz="11" staPduPrsnt="0"/>
      </tapa.expect>

 
