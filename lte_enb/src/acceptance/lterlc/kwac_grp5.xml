#
#
#       Test:    5.1
#
#       Desc:    Verification of sequence number at the transmission side (SN = 5 bits) 
#
#       Notes:   None
#              
#       Module:  RLC 
#
#       File:    kwac_grp5.xml
#
#
<tapa.testcase id="5.1">

   # Document subsection
   <tapa.doc>

      <tapa.rfc id="None" section="None"/>
      <tapa.module id="rlc"/>

      <tapa.desc>
         "Verification of sequence number at the transmission side"
      </tapa.desc>

      <tapa.longdesc>
         " Acceptance test case No: 5.1
           1. Configure RLC through LM
           2. Configure RLC entities
           3. PDCP sends 3 SDU'S  
           4. RLC sends StaRes()
           5. MAC gives BO
           6. RLC sends 3 SDU'S to MAC
           7. MAC verifies the sequence number of each PDU
          " 
      </tapa.longdesc>
         
   </tapa.doc>

   # Require features and flags subsection 
   <tapa.require>

      <tapa.ifdef/>
        
      <tapa.ifndef/>
         
      <tapa.mode mode="single"/>

   </tapa.require>

   # Configuration subsection
    <tapa.cfg>
      <tapa.kw.init/>
      <tapa.kw.umcfg1/>
   </tapa.cfg>
     
   # Test Steps subsection
   <tapa.test>
    
      #PDCP sends SDU with size as 300 
      <tapa.send>
         <tapa.kwu.datreq entity="ENTPJ" sduId="1" sduSize="300"
            rbId="1" rbType="1" ueId="1" cellId="1"/>
      </tapa.send>
 
      #MAC expects the Status Response from RLC
      <tapa.expect>
         <tapa.dummy.rgu.dstarsp cellId="1" ueId="1" lcId="1" 
               bo="300" estHdrSz="3" staPduPrsnt="0"/>
      </tapa.expect>
   
      #MAC notifies BO with size as 304 
      <tapa.send>
         <tapa.dummy.rgu.dstaind totBufSize="304" lcId="1" ueId="1" cellId="1" 
               numLch="1" transId="1" />
      </tapa.send>

      #MAC expects data request from RLC 
      #verifyType=1 -> sequence number is in order 
      <tapa.expect>
         <tapa.dummy.rgu.ddatreq numLch="1" ueId="1" cellId="1" transId="1"
               lchData="0:0:0:1:1:301:
                        0:0:1:0:0"/>
      </tapa.expect>
      
      #PDCP sends SDU with size as 300 
      <tapa.send>
         <tapa.kwu.datreq entity="ENTPJ" sduId="2" sduSize="300"
            rbId="1" rbType="1" ueId="1" cellId="1"/>
      </tapa.send>
      
      #MAC expects the Status Response from RLC
      <tapa.expect>
         <tapa.dummy.rgu.dstarsp cellId="1" ueId="1" lcId="1" 
               bo="300" estHdrSz="3" staPduPrsnt="0"/>
      </tapa.expect>
   
      #MAC notifies BO with size as 304 
      <tapa.send>
         <tapa.dummy.rgu.dstaind totBufSize="304" lcId="1" ueId="1" cellId="1" 
               numLch="1" transId="2" />
      </tapa.send>

      #MAC expects data request from RLC
      <tapa.expect>
         <tapa.dummy.rgu.ddatreq numLch="1" ueId="1" cellId="1" transId="2"
               lchData="0:0:0:1:1:301:
                        1:0:1:0:0"/>
      </tapa.expect>

      #PDCP sends SDU with size as 300
      <tapa.send>
         <tapa.kwu.datreq entity="ENTPJ" sduId="3" sduSize="300"
            rbId="1" rbType="1" ueId="1" cellId="1" />
      </tapa.send>

      #MAC expects the Status Response from RLC
      <tapa.expect>
         <tapa.dummy.rgu.dstarsp  cellId="1" ueId="1" lcId="1" 
               bo="300" estHdrSz="3" staPduPrsnt="0"/>
      </tapa.expect>

      #MAC notifies BO with size as 304
      <tapa.send>
         <tapa.dummy.rgu.dstaind totBufSize="304" lcId="1" ueId="1" cellId="1" 
               numLch="1" transId="3"/>
      </tapa.send>

      #MAC expects data request from RLC
      <tapa.expect>
         <tapa.dummy.rgu.ddatreq numLch="1" ueId="1" cellId="1" transId="3"
               lchData="0:0:0:1:1:301:
                        2:0:1:0:0"/>
      </tapa.expect>
      
      #MAC indicates Data Indication to Peer RLC
      <tapa.send>
         <tapa.dummy.rgu.ddatind numLch="1" cellId="1"  ueId="2" transId="1"
               lchData="1:1:0:0:0:0"/>
      </tapa.send>

      #RLC Indicates Data Indication to Peer PDCP
      <tapa.expect>
         <tapa.kwu.datind entity="ENTPJ" 
               cellId="1" ueId="2" rbId="1" rbType="1"/>
      </tapa.expect>

      #MAC indicates Data Indication to Peer RLC
      <tapa.send>
         <tapa.dummy.rgu.ddatind numLch="1" cellId="1"  ueId="2" transId="2"
               lchData="1:1:0:0:0:0"/>
      </tapa.send>

      #RLC Indicates Data Indication to Peer PDCP
      <tapa.expect>
         <tapa.kwu.datind entity="ENTPJ" 
               cellId="1" ueId="2" rbId="1" rbType="1"/>
      </tapa.expect>

      #MAC indicates Data Indication to Peer RLC
      <tapa.send>
         <tapa.dummy.rgu.ddatind numLch="1" cellId="1"  ueId="2" transId="3"
               lchData="1:1:0:0:0:0"/>
      </tapa.send>

      #RLC Indicates Data Indication to Peer PDCP
      <tapa.expect>
         <tapa.kwu.datind entity="ENTPJ" 
               cellId="1" ueId="2" rbId="1" rbType="1"/>
      </tapa.expect>

      <tapa.delay delay="5"/> 
   </tapa.test>

   # Clean subsection
   <tapa.clean/>
         
   # Shutdown subsectitn
   <tapa.shutdown>
   
   <tapa.kw.deinit/>

   <tapa.delay delay="5"/> 
      
   </tapa.shutdown>
</tapa.testcase>


