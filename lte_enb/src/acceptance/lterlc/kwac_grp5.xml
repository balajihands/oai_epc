               lchData="0:0:0:1:1:604:
                        0:0:0:0:0"/>
      </tapa.expect>

      #MAC indicates Data Indication to Peer RLC
      <tapa.send>
         <tapa.dummy.rgu.ddatind numLch="1" cellId="1"  ueId="2" transId="1"
               lchData="1:1:0:0:0:0"/>
      </tapa.send>

      #RLC Indicates Data Indication to Peer PDCP
      <tapa.expect>
         <tapa.kwu.datind entity="ENTPJ" sduSize="200" 
               cellId="1" ueId="2" rbId="1" rbType="1"/>
      </tapa.expect>

      #RLC Indicates Data Indication to Peer PDCP
      <tapa.expect>
         <tapa.kwu.datind entity="ENTPJ" sduSize="200" 
               cellId="1" ueId="2" rbId="1" rbType="1"/>
      </tapa.expect>

      #RLC Indicates Data Indication to Peer PDCP
      <tapa.expect>
         <tapa.kwu.datind entity="ENTPJ" sduSize="200" 
               cellId="1" ueId="2" rbId="1" rbType="1"/>
      </tapa.expect>

