#
#
#       Test:    5.5
#
#       Desc:    Unsuccessful SDU discard 
#
#       Notes:   None
#              
#       Module:  rlc 
#
#       File:    kwac_grp5.xml
#
#
<tapa.testcase id="5.5">

   # Document subsection
   <tapa.doc>

      <tapa.rfc id="None" section="None"/>
      <tapa.module id="rlc"/>

      <tapa.desc>
         "Unsuccessful SDU discard (Partial SDU should not Discard)"
      </tapa.desc>

      <tapa.longdesc>
          "Acceptance test case No: 5.5
           1. Configure RLC through LM.
           2. Configure UM RLC entities.
           3. Tx PDCP send 1 SDU size of 600 to Tx RLC.
           4. MAC send DStaInd to Tx RLC with BO=305.
           5. Tx RLC sends DDatReq to MAC with PDU of size 301.
           6. Tx PDCP send DiscSdu to Tx RLC. Tx RLC should not discard SDU
           (Partial).
           7. Verify through the Statistics Request and Confirm."
      </tapa.longdesc>
         
   </tapa.doc>

   # Require features and flags subsection 
   <tapa.require>
      
      <tapa.ifdef/>
        
      <tapa.ifndef/>

      <tapa.mode mode="single"/>

   </tapa.require>

   # Configuration subsection
   <tapa.cfg>
      <tapa.kw.init/>
      <tapa.kw.umcfg1/>
   </tapa.cfg>

   # Test Steps subsection
   <tapa.test>
      <tapa.send>
         <tapa.kwu.datreq entity="ENTPJ" sduId="4" sduSize="600" rbId="1"
            rbType="1" ueId="1" cellId="1" />
      </tapa.send>
      
      <tapa.expect>
         <tapa.dummy.rgu.dstarsp cellId="1" ueId="1" lcId="1" 
               bo="600" estHdrSz="3" staPduPrsnt="0"/>
      </tapa.expect>

      <tapa.send>
         <tapa.dummy.rgu.dstaind totBufSize="304" lcId="1" ueId="1" cellId="1" 
               numLch="1" transId="1"/>
      </tapa.send>

      <tapa.expect>
         <tapa.dummy.rgu.ddatreq numLch="1" ueId="1" cellId="1" lcId="1" transId="1"
               lchData="300:3:0:1:1:301:
                        -1:-1:-1:-1:-1:-1"/>
      </tapa.expect>
      
      <tapa.send>
          <tapa.kwu.discsdu rbId="1" rbType="1" ueId="1" cellId="1" sduId="0"/>
      </tapa.send>

      #Invalid Rb Id in SDU discard
      <tapa.send>
          <tapa.kwu.discsdu rbId="9" rbType="1" ueId="1" cellId="1" sduId="0"/>
      </tapa.send>
      
      <tapa.send>
         <tapa.dummy.rgu.dstaind totBufSize="304" lcId="1" ueId="1" cellId="1" 
               numLch="1" transId="1"/>
      </tapa.send>

      <tapa.expect>
         <tapa.dummy.rgu.ddatreq numLch="1" ueId="1" cellId="1" lcId="1" transId="1"
               lchData="0:0:0:1:1:301:-1:-1:-1:-1:-1:-1"/>
      </tapa.expect>
      
      <tapa.send>
           <tapa.lkw.stsreq elmnt="1" action="1" suId="1" sapType="1" inst="0"/>
      </tapa.send>

      #evnki - Start
      # servType = 1 -> verify sdu is discarded or not
      #kw005.201 ccpu00117318, updating the statistics.
      <tapa.expect>
           <tapa.lkw.stscfm elmnt="1" numUe="2" numOfRb="2" pdusSent="2" pdusRetx="0"
        bytesSent="602" protTimeOut="0" numSduDisc="0" servType="1" inst="0"/>
      </tapa.expect>

      <tapa.send>
           <tapa.lkw.stsreq elmnt="1" action="1" suId="1" sapType="1" inst="1"/>
      </tapa.send>

      # servType = 1 -> verify sdu is discarded or not
      #kw005.201 ccpu00117318, updating the statistics.
      <tapa.expect>
           <tapa.lkw.stscfm elmnt="1" numUe="2" numOfRb="2" pdusRecv="0"
           bytesRecv="0" unexpPdusRecv="0" errorPdusRecv="0" 
           servType="1" inst="1"/>
      </tapa.expect>
      <tapa.delay delay="5"/>
   #Venki - end 

   </tapa.test>

   # Clean subsection
   <tapa.clean/>

   # Shutdown subsectitn
   <tapa.shutdown>
   <tapa.kw.deinit/>

   <tapa.delay delay="5"/> 

   </tapa.shutdown>
</tapa.testcase>

