#
#
#       Test:    3.25
#
#       Desc:    Successful paging for a UE in Idle Mode and then  rrc connection
#                is established as per the trigger from UE. Now send a paging
#                message to notify the CMAS indication  
#
#       Notes:   None
#              
#       Module:  rrc.pag
#
#       File:    nhac_tc3.xml
#
#
<tapa.testcase id="3.25">

   # Document subsection
   <tapa.doc>

      <tapa.rfc id="36.331" section="5.3.2"/>
      <tapa.module id="lterrc"/>

      <tapa.desc>
           "Pretest Cell Config"
           "Send paging message for a UE, with UE-ID=1, which is in idle mode 
            from RRC User. RRC should process this message and send it to UE. 
            UE will respond with connection request message. RRC should 
            process connecton request, and Successfully conection 
            should be established with UE. Activate the initial security and send 
            a paging message to notify the CMAS indication"
      </tapa.desc>

      <tapa.longdesc/>

   </tapa.doc>

   # Require features and flags subsection
   <tapa.require>

      <tapa.ifdef>
        <tapa.flag id="LTERRC_REL9"/>
      </tapa.ifdef>

      <tapa.ifndef/>
      <tapa.mode mode="single"/>

   </tapa.require>

   # Configuration subsection
   <tapa.cfg>
      # Bring up the RRC layer using LM
      <tapa.rrc.init />
   </tapa.cfg>

   <tapa.test>
      # Bring up the cell and common resources
      <tapa.rrc.cellcfg cellId="1"/>

      # Send Paging message to RRC. RRC will send the message to 
      # RLC on KWU interface
      <tapa.send>
         <tapa.nhu.datreq spId="0" bitMask="0" msgCategory="5"
          rbId="1" sfn="10" subFrmNo="2" pagingCause="0" 
          numOfUe="1" domainIdent="0" cellId="1" ueId="1" isCMASIndEnabled="1"/>
      </tapa.send>

      <tapa.delay delay="5" />

      # Expect paging message in data request from RRC to RLC 
      # on KWU interface
      <tapa.expect>
         <tapa.dummy.kwu.datreq spId="0" cellId="1" rbId="1" sfn="10" 
                          subFrmNo="2" ueId="1"/>
      </tapa.expect>

      # Establish a rrc connection for the UE
      <tapa.rrc.consetup ueId="1" cellId="1" tCrnti="1"/>
      
      # Security Activation for UE in connected mode
      <tapa.rrc.security_actv ueId="1" cellId="1"/>

      # Send Paging message to RRC. RRC will send the message to 
      # RLC on KWU interface
      <tapa.send>
         <tapa.nhu.datreq spId="0" bitMask="0" msgCategory="5"
          rbId="1" sfn="10" subFrmNo="2" pagingCause="0" 
          numOfUe="1" domainIdent="0" cellId="1" ueId="1" isCMASIndEnabled="1" />
      </tapa.send>

      <tapa.delay delay="5" />

      # Expect data request from RRC to RLC on KWU interface
      <tapa.expect>
         <tapa.dummy.kwu.datreq spId="0" cellId="1" rbId="1" lcType="2" sfn="10" 
                          subFrmNo="2" ueId="1"/>
      </tapa.expect>

      <tapa.delay delay="5" />

   </tapa.test>

   # Clean subsection
   <tapa.clean>
   </tapa.clean>

   # Shutdown subsection
   <tapa.shutdown>
      <tapa.rrc.deinit/>
   </tapa.shutdown>

</tapa.testcase>


