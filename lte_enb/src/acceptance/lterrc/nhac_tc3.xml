      # Bring up the RRC layer using LM
      <tapa.rrc.init />
   </tapa.cfg>

   <tapa.test>
      # Configure cell1 and common resources
      <tapa.rrc.cellcfg cellId="1"/>

      # Send Paging message to RRC. RRC will not send the message to 
      # RLC as the tbsSize passed is 0, which is lesser than the
      # length of the paging message after encoding.
