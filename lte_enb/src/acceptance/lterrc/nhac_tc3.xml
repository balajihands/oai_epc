#       Test:    3.5
#
#       Desc:    Un-successful paging for a UE in Idle Mode
#                due to incorrect tbsSize.
#
#       Notes:   None
#              
#       Module:  rrc.pag
#
#       File:    nhac_tc3.xml
#
#
<tapa.testcase id="3.5">

   # Document subsection
   <tapa.doc>

      <tapa.rfc id="None" section="None"/>
      <tapa.module id="lterrc"/>

      <tapa.desc>
           "Pretest Cell Configuration"
           "Send paging message for a UE, with UE-ID=1, which is in Idle mode,
            from RRC User. RRC encodes this paging message but will not send to 
            RLC as the tbsSize received from RRC user is smaller than the encoded
            length."
      </tapa.desc>

      <tapa.longdesc/>

   </tapa.doc>

   # Require features and flags subsection
   <tapa.require>

      <tapa.ifdef>
      </tapa.ifdef>

      <tapa.ifndef>
