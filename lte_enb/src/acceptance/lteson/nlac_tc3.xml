          <tapa.lnl.cfgUpdateInd />
      </tapa.expect>
      # Expect CFG Update for PCI
      <tapa.expect>
          <tapa.lnl.cfgUpdateInd />
      </tapa.expect>
      # Expect CFG Update for TxPower
      <tapa.expect>
          <tapa.lnl.cfgUpdateInd />
      </tapa.expect>
      # Expect Neighbor Cell Cfg Cfm
      <tapa.expect>
         <tapa.lnl.nghCellCfgCfm />
      </tapa.expect>
      #Cell ReCFG for LTE-SON
      <tapa.send>
         <tapa.lnl.cellReCfgReq coupling = "1" cellId = "1" mcc_0 = "2" mcc_1 = "2" mcc_2 = "0" mnc_0 = "2" mnc_1 = "0" 
                                 numPci = "10" pci_0 = "1" pci_1 = "3" pci_2 = "4"
                                 pci_3 = "5" pci_4 = "100" pci_5 = "110" pci_6 = "120" pci_7 = "130" pci_8 = "150" pci_9 = "160"/>
      </tapa.send>
      # Expect CFG Update for PCI
      <tapa.expect>
          <tapa.lnl.cfgUpdateInd />
      </tapa.expect>
      # Expect Cell ReCfg Cfm
      <tapa.expect>
         <tapa.lnl.cellReCfgCfm />
      </tapa.expect>
      # ShutDown request
      <tapa.send >
         <tapa.lnl.shutdownReq  coupling="1" />
      </tapa.send>
      # Expect shutdown Confirm from SON to LM
      <tapa.expect>
         <tapa.lnl.shutdownCfm/>
      </tapa.expect>
   </tapa.test>
   # Clean subsection
   <tapa.clean/>
   # Shutdown subsection
   <tapa.shutdown>
   </tapa.shutdown>
</tapa.testcase>

