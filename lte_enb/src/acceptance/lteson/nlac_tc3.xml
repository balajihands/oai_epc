      <tapa.expect>
          <tapa.lnl.cfgUpdateInd />
      </tapa.expect>
      # Expect Cell Configuration Confirm from SON to LM
      <tapa.expect>
         <tapa.lnl.cellCfgCfm />
      </tapa.expect>
      #Neighbor Cell CFG for LTE-SON
      <tapa.send>
         <tapa.lnl.nghCellCfgReq coupling = "1" cellId = "1" numNghEnb = "1" nghEnbId_0 = "2" nghEnbId_0_cfgType = "0" nghEnbId_0_numAdd = "1"
