                                 nghEnbId_0_add_0_mnc_0 = "2" nghEnbId_0_add_0_mnc_1 = "0" nghEnbId_0_add_0_pci = "100" nghEnbId_0_add_0_dlEarfcn = "3500"
                                 nghEnbId_0_add_0_ulEarfcn = "1110" nghEnbId_0_add_0_numNghNgh = "0" />
