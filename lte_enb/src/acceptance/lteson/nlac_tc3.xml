#
#       Test:    3.1
#
#       Desc:    In NL_CELL_STATE_CONFIGURED, PCI list modification through cell reconfiguration 
#                from SM and serving cell PCI present in PCI list in new PCIList.   
#
#       Notes:   None
#
#       Module:
#
#       File:    nlac_tc3.xml
#
#
<tapa.testcase id="3.1">

   # Document subsection
   <tapa.doc>
      <tapa.rfc id="None" section="None"/>
      <tapa.module id="SON"/>
      <tapa.desc>
         "SON Generic Configurations"
         "SON NLU SAP Configurations"
         "SON Cell Configurations with PCI, EARFCN,PRACH selection flag enable"
      </tapa.desc>
      <tapa.longdesc/>
   </tapa.doc>
   # Require features and flags subsection
   <tapa.require>
      <tapa.ifdef/>
      <tapa.ifndef/>
      <tapa.mode mode="single"/>
   </tapa.require>
   # Configuration subsection
   <tapa.cfg/>
   # Test Steps subsection
   <tapa.test>
      #Gen Cfg for LTE-SON
      <tapa.send >
         <tapa.lnl.genCfgReq  coupling="1" pciSelEnable = "1" earfcnSelEnable = "0" prachSelEnable = "0"/>
      </tapa.send>
      # Expect General Configuration Confirm from SON to LM
      <tapa.expect>
         <tapa.lnl.genCfgCfm/>
      </tapa.expect>
      #NLU SAP configuration for LTE-SON
      <tapa.send >
         <tapa.nlu.sapCfgReq coupling="1" />
      </tapa.send>
      # Expect SAP Configuration Confirm from SON to LM
      <tapa.expect>
         <tapa.nlu.sapCfgCfm/>
      </tapa.expect>
      #Cell Cfg for LTE-SON
      <tapa.send >
         <tapa.lnl.cellCfgReq coupling="1" cellId="1" bandWidth="20" antPorts="2" mcc_0 = "2" mcc_1 = "2" mcc_2 = "0" mnc_0 = "2" mnc_1 = "0" 
                              txPower="0" subFrameCfg="1" pci="1" numPci = "1" pci_0 = "1" pci_1 = "3" pci_2 = "4" 
                              pci_3 = "5" pci_4 = "100" pci_5 = "110" pci_6 = "120" pci_7 = "130" pci_8 = "150" pci_9 = "160" numDlEarfcn = "5"
