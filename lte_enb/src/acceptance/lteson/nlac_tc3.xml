                              numRootSeqIndex="5" rootSeqIdx_0="1" rootSeqIdx_1="2" rootSeqIdx_2="3" rootSeqIdx_3="4" rootSeqIdx_4="5"
                              numPrachCfgIdx="5" prachConfigIdx_0="1" prachConfigIdx_1="2" prachConfigIdx_2="3" prachConfigIdx_3="4" prachConfigIdx_4="5"
                              numZeroCorrCfgZone="5" zeroCorrelationZoneCfg_0="1" zeroCorrelationZoneCfg_1="2" zeroCorrelationZoneCfg_2="3"
                              zeroCorrelationZoneCfg_3="4" zeroCorrelationZoneCfg_4="5"
                              numFreqOffset="5" freqOffset_0="1" freqOffset_1="2" freqOffset_2="3" freqOffset_3="4" freqOffset_4="5" />
