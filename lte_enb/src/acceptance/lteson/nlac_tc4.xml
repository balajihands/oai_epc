<tapa.testcase id="4.34">

   # Document subsection
   <tapa.doc>
      <tapa.rfc id="None" section="None"/>
      <tapa.module id="SON"/>
      <tapa.desc>
         "SON Generic Configurations"
         "SON NLU SAP Configurations"
         "SON Cell Configurations with PCI, EARFCN,PRACH selection flag enable"
      </tapa.desc>
      <tapa.longdesc/>
   </tapa.doc>
   # Require features and flags subsection
   <tapa.require>
      <tapa.ifdef/>
      <tapa.ifndef/>
      <tapa.mode mode="single"/>
   </tapa.require>
   # Configuration subsection
   <tapa.cfg/>
   # Test Steps subsection
   <tapa.test>
      #Gen Cfg for LTE-SON
      <tapa.send >
         <tapa.lnl.genCfgReq  coupling="1" pciSelEnable = "1" earfcnSelEnable = "0" prachSelEnable = "0"/>
      </tapa.send>
      # Expect General Configuration Confirm from SON to LM
      <tapa.expect>
         <tapa.lnl.genCfgCfm/>
      </tapa.expect>
      #NLU SAP configuration for LTE-SON
      <tapa.send >
         <tapa.nlu.sapCfgReq coupling="1" />
      </tapa.send>
      # Expect SAP Configuration Confirm from SON to LM
      <tapa.expect>
         <tapa.nlu.sapCfgCfm/>
      </tapa.expect>
      #Cell Cfg for LTE-SON
      <tapa.send >
         <tapa.lnl.cellCfgReq coupling="1" cellId="1" bandWidth="20" antPorts="2" mcc_0 = "2" mcc_1 = "2" mcc_2 = "0" mnc_0 = "2" mnc_1 = "0" 
                              txPower="0" subFrameCfg="1" pci="1" numPci = "2" pci_0 = "1" pci_1 = "3" pci_2 = "4" 
                              pci_3 = "5" pci_4 = "100" pci_5 = "110" pci_6 = "120" pci_7 = "130" pci_8 = "150" pci_9 = "160" numDlEarfcn = "5"
                              dlEarfcn_0 = "3500" dlEarfcn_1 = "4500" dlEarfcn_2 = "1500" dlEarfcn_3 = "2500" dlEarfcn_4 = "7500" numUlEarfcn = "5" 
                              ulEarfcn_0 = "1110" ulEarfcn_1 = "1120" ulEarfcn_2 = "1130" ulEarfcn_3 = "1140" ulEarfcn_4 = "1150" numRemEarfcn = "2"
                              rem_dlEarfcn_0 = "5500" rem_rssi_0 = "15" rem_dlEarfcn_1 = "6500" rem_rssi_1 = "10" isCpUlExtend="1" 
                              numRootSeqIndex="5" rootSeqIdx_0="1" rootSeqIdx_1="2" rootSeqIdx_2="3" rootSeqIdx_3="4" rootSeqIdx_4="5"
                              numPrachCfgIdx="5" prachConfigIdx_0="1" prachConfigIdx_1="2" prachConfigIdx_2="3" prachConfigIdx_3="4" prachConfigIdx_4="5"
                              numZeroCorrCfgZone="5" zeroCorrelationZoneCfg_0="1" zeroCorrelationZoneCfg_1="2" zeroCorrelationZoneCfg_2="3"
                              zeroCorrelationZoneCfg_3="4" zeroCorrelationZoneCfg_4="5"
                              numFreqOffset="5" freqOffset_0="1" freqOffset_1="2" freqOffset_2="3" freqOffset_3="4" freqOffset_4="5" />
      </tapa.send>
      # Expect CFG Update for EARFCN
      <tapa.expect>
          <tapa.lnl.cfgUpdateInd />
      </tapa.expect>
      # Expect Cell Configuration Confirm from SON to LM
      <tapa.expect>
         <tapa.lnl.cellCfgCfm />
      </tapa.expect>
      #Neighbor Cell CFG for LTE-SON
      <tapa.send>
         <tapa.lnl.nghCellCfgReq coupling = "1" cellId = "1" numNghEnb = "1" nghEnbId_0 = "2" nghEnbId_0_cfgType = "0" nghEnbId_0_numAdd = "1"
                                 nghEnbId_0_add_0_cellId = "2" nghEnbId_0_add_0_mcc_0 = "2" nghEnbId_0_add_0_mcc_1 = "2" nghEnbId_0_add_0_mcc_2 = "0"
                                 nghEnbId_0_add_0_mnc_0 = "2" nghEnbId_0_add_0_mnc_1 = "0" nghEnbId_0_add_0_pci = "100" nghEnbId_0_add_0_dlEarfcn = "3500"
                                 nghEnbId_0_add_0_ulEarfcn = "1110" nghEnbId_0_add_0_numNghNgh = "0"/>
      </tapa.send>
      # Expect CFG Update for PRACH
      <tapa.expect>
          <tapa.lnl.cfgUpdateInd />
      </tapa.expect>
      # Expect CFG Update for PCI
      <tapa.expect>
          <tapa.lnl.cfgUpdateInd />
      </tapa.expect>
      # Expect CFG Update for TxPower
      <tapa.expect>
          <tapa.lnl.cfgUpdateInd />
      </tapa.expect>
      # Expect Neighbor Cell Cfg Cfm
      <tapa.expect>
         <tapa.lnl.nghCellCfgCfm />
      </tapa.expect>
      #X2 setup req Neighbor Cell CFG for LTE-SON
      <tapa.send>
         <tapa.nlu.nghReq coupling = "1" msgType="0" cellId = "1" numNghEnb = "1" nghEnbId_0 = "2" nghEnbId_0_cfgType = "0" nghEnbId_0_numAdd = "1"
                                 nghEnbId_0_add_0_cellId = "2" nghEnbId_0_add_0_mcc_0 = "2" nghEnbId_0_add_0_mcc_1 = "2" nghEnbId_0_add_0_mcc_2 = "0"
                                 nghEnbId_0_add_0_mnc_0 = "2" nghEnbId_0_add_0_mnc_1 = "0" nghEnbId_0_add_0_pci = "100" nghEnbId_0_add_0_dlEarfcn = "3500"
                                 nghEnbId_0_add_0_ulEarfcn = "1110" nghEnbId_0_add_0_numNghNgh = "0" />
      </tapa.send>
      #X2 setup rsp Neighbor Cell CFG for LTE-SON
      <tapa.send>
         <tapa.nlu.nghReq coupling = "1" msgType="1" cellId = "1" numNghEnb = "1" nghEnbId_0 = "3" nghEnbId_0_cfgType = "0" nghEnbId_0_numAdd = "1"
                                 nghEnbId_0_add_0_cellId = "3" nghEnbId_0_add_0_mcc_0 = "2" nghEnbId_0_add_0_mcc_1 = "2" nghEnbId_0_add_0_mcc_2 = "0"
                                 nghEnbId_0_add_0_mnc_0 = "2" nghEnbId_0_add_0_mnc_1 = "0" nghEnbId_0_add_0_pci = "3" nghEnbId_0_add_0_dlEarfcn = "3500"
                                 nghEnbId_0_add_0_ulEarfcn = "1110" nghEnbId_0_add_0_numNghNgh = "0" />
      </tapa.send>
      # Expect PCI collision alarm
      <tapa.expect>
          <tapa.lnl.statusInd />
      </tapa.expect>
      # Expect CFG Update for PCI
      <tapa.expect>
          <tapa.lnl.cfgUpdateInd />
      </tapa.expect>
      #X2 setup rsp Neighbor Cell CFG for LTE-SON
      <tapa.send>
         <tapa.nlu.nghReq coupling = "1" msgType="1" cellId = "1" numNghEnb = "1" nghEnbId_0 = "4" nghEnbId_0_cfgType = "0" nghEnbId_0_numAdd = "1"
                                 nghEnbId_0_add_0_cellId = "4" nghEnbId_0_add_0_mcc_0 = "2" nghEnbId_0_add_0_mcc_1 = "2" nghEnbId_0_add_0_mcc_2 = "0"
                                 nghEnbId_0_add_0_mnc_0 = "2" nghEnbId_0_add_0_mnc_1 = "0" nghEnbId_0_add_0_pci = "20" nghEnbId_0_add_0_dlEarfcn = "3500"
                                 nghEnbId_0_add_0_ulEarfcn = "1110" nghEnbId_0_add_0_numNghNgh = "1" nghEnbId_0_add_0_nghNgh_0_cellId = "5"
                                 nghEnbId_0_add_0_nghNgh_0_mcc_0 = "2" nghEnbId_0_add_0_nghNgh_0_mcc_1 = "2" nghEnbId_0_add_0_nghNgh_0_mcc_2 = "0"
                                 nghEnbId_0_add_0_nghNgh_0_mnc_0 = "2" nghEnbId_0_add_0_nghNgh_0_mnc_1 = "0" nghEnbId_0_add_0_nghNgh_0_pci = "3"
                                 nghEnbId_0_add_0_nghNgh_0_dlEarfcn = "3500" nghEnbId_0_add_0_nghNgh_0_ulEarfcn = "1110"/>
      </tapa.send>
      #PCI mod confirm Indication
      <tapa.send>
         <tapa.nlu.pciModIndCfm  cellId = "1" mcc_0 = "2" mcc_1 = "2" mcc_2 = "0" mnc_0 = "2" mnc_1 = "0" pci="5"/>
      </tapa.send>
      # Expect PCI collision clear alarm
      <tapa.expect>
          <tapa.lnl.statusInd />
      </tapa.expect>
      # Expect PCI confusion alarm
      <tapa.expect>
          <tapa.lnl.statusInd />
      </tapa.expect>
      # ShutDown request
      <tapa.send >
         <tapa.lnl.shutdownReq  coupling="1" />
      </tapa.send>
      # Expect shutdown Confirm from SON to LM
      <tapa.expect>
         <tapa.lnl.shutdownCfm/>
      </tapa.expect>
   </tapa.test>
   # Clean subsection
   <tapa.clean/>
   # Shutdown subsection
   <tapa.shutdown>
   </tapa.shutdown>
</tapa.testcase>


