#
#
#       Test:    tapa.hi.init
#
#       Desc:    Do general configuration
#
#       Notes:   None
#
#       Module:  None
#
#       File:    szac_cmd1.xml
#
#
<tapa.command id="tapa.hi.init">

   # Document subsection
   <tapa.doc>

      <tapa.desc>
         "Do TUCL initialisation"
      </tapa.desc>

      <tapa.longdesc/>

   </tapa.doc>

   # Steps subsection
   <tapa.steps>

      #configure the TUCL
      # Send General Configuration Request
      <tapa.send>
         <tapa.lhi.cfg.gen/>
      </tapa.send>

      # Expect General Configuration Confirm
      <tapa.expect>
         <tapa.lhi.cfgcfm/>
      </tapa.expect>

      # Send TSAP Configuration Request
      <tapa.send>
         <tapa.lhi.cfg.tsap tSapId="0"/>
      </tapa.send>
      
      # Expect TSAP Configuration Confirm
      <tapa.expect>
         <tapa.lhi.cfgcfm/>
      </tapa.expect>

      # Send TSAP Configuration Request
      <tapa.send flag="SZ_KSCTP">
         <tapa.lhi.cfg.tsap tSapId="1"/>
      </tapa.send>
      
      # Expect TSAP Configuration Confirm
      <tapa.expect flag="SZ_KSCTP">
         <tapa.lhi.cfgcfm/>
      </tapa.expect>


      #configure the TUCL2 for INTEGRATED TESTING
      # Send General Configuration Request
      <tapa.send flag="SZAC_INTG_TEST">
         <tapa.lhi.cfg.gen procId="SZAC_MME_PROC"/>
      </tapa.send>

      # Expect General Configuration Confirm
      <tapa.expect flag="SZAC_INTG_TEST">
         <tapa.lhi.cfgcfm procId="SZAC_MME_PROC"/>
      </tapa.expect>

      # Send TSAP Configuration Request
      <tapa.send flag="SZAC_INTG_TEST">
         <tapa.lhi.cfg.tsap tSapId="0" procId="SZAC_MME_PROC"/>
      </tapa.send>
      
      # Expect TSAP Configuration Confirm
      <tapa.expect flag="SZAC_INTG_TEST">
         <tapa.lhi.cfgcfm procId="SZAC_MME_PROC"/>
      </tapa.expect>

   </tapa.steps>

</tapa.command>

#
#       Command: tapa.hi.deinit
#
#       Desc:    Command to perform shutdown for TUCL.
#
#       Notes:   None
#              
#       Module:  S1AP
#
#       File:    xx_cmd1.xml
#
#

<tapa.command id="tapa.hi.deinit">
   
   # Document subsection
