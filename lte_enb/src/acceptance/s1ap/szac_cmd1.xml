      <tapa.desc>
         "Initialize dummy proxy node"
      </tapa.desc>

      <tapa.longdesc/>

   </tapa.doc>

   # Steps subsection
   <tapa.steps>

      # Send an General configuration request
      <tapa.send>
         <tapa.lsz.cfg.gen />
      </tapa.send>

      # Receive the confirm for the general configuration request.
      <tapa.expect>
         <tapa.lsz.cfgcfm />
      </tapa.expect>

      # Send an protocol configuration request
      <tapa.send>
         <tapa.lsz.cfg.prot protNode="SZAC_LSZ_GW_PRC2"/>
      </tapa.send>

      # Receive the confirm for the protocol configuration request.
      <tapa.expect>
         <tapa.lsz.cfgcfm />
      </tapa.expect>

      # Send an szt sap configuration request
      <tapa.send>
         <tapa.lsz.cfg.usap />
      </tapa.send>

      # Receive the confirm for the szt sap request.
      <tapa.expect>
         <tapa.lsz.cfgcfm />
      </tapa.expect>

      # Send an sct sap configuration request
      <tapa.send>
         <tapa.lsz.cfg.lsap peerNode="SZAC_LSZ_GW_PRC2" provType="SZAC_SCTP_DM"/>
      </tapa.send>

      # Receive the confirm for the sct sap request.
      <tapa.expect>
         <tapa.lsz.cfgcfm />
      </tapa.expect>

      # Send an sct sap configuration request
      <tapa.send>
         <tapa.lsz.cfg.lsap suId="1" spId="1" peerNode="SZAC_LSZ_GW_PRC2" provType="SZAC_SCTP_DM"/>
      </tapa.send>

      # Receive the confirm for the sct sap request.
      <tapa.expect>
         <tapa.lsz.cfgcfm />
      </tapa.expect>

      # Send a MME peer configuration request
      <tapa.send>
         <tapa.lsz.cfg.peer peerNodeCfg="SZAC_LSZ_MME_PRC1"/>
      </tapa.send>

      # Receive the confirm for the peer configuration request.
      <tapa.expect>
         <tapa.lsz.cfgcfm />
      </tapa.expect>

      # Send a eNB peer configuration request
      <tapa.send>
         <tapa.lsz.cfg.peer uSapId="0" sapId="1" peerNodeCfg="SZAC_LSZ_ENB_PRC0"/>
      </tapa.send>

      # Receive the confirm for the peer configuration request.
      <tapa.expect>
         <tapa.lsz.cfgcfm />
      </tapa.expect>

      # Send an sct sap configuration request.
      <tapa.send>
         <tapa.lsz.cntrl.lSap />
      </tapa.send>

      <tapa.expset>
         # Receive the bind request and confirm for the sct sap request.
         <tapa.sct.bndreq spId="0"/> 
         # Receive the confirm for the sct sap request.
         <tapa.lsz.cntrlcfm/>
      </tapa.expset>

      # Send bind cfm.
      <tapa.send>
         <tapa.sct.bndcfm procId="SZAC_ENB_PROC" spId="0" suId="0" result="1"/> 
      </tapa.send>

      # Expect EndpOpen Req to dummy SCTP
      <tapa.expect>
         <tapa.sct.endpopenreq procId="SZAC_ENB_PROC" spId="0" suEndpId="0"/> 
      </tapa.expect>
  
      # Send EndpOpen Cfm at eNB 
      <tapa.send>
         <tapa.sct.endpopencfm procId="SZAC_ENB_PROC" suId="0" suEndpId="0" spEndpId="0" /> 
      </tapa.send>

      # Send an sct sap configuration request.
      <tapa.send>
         <tapa.lsz.cntrl.lSap lSapId="1"/>
      </tapa.send>

      <tapa.expset>
         # Receive the bind request and confirm for the sct sap request.
         <tapa.sct.bndreq spId="1" suId="1"/> 
         # Receive the confirm for the sct sap request.
         <tapa.lsz.cntrlcfm/>
      </tapa.expset>

      # Send bind cfm.
      <tapa.send>
         <tapa.sct.bndcfm procId="SZAC_ENB_PROC" spId="1" suId="1" result="1"/> 
      </tapa.send>

      # Expect EndpOpen Req to dummy SCTP
      <tapa.expect>
         <tapa.sct.endpopenreq procId="SZAC_ENB_PROC" spId="1" suId="1" suEndpId="1"/> 
      </tapa.expect>
  
      # Send EndpOpen Cfm at eNB 
      <tapa.send>
         <tapa.sct.endpopencfm procId="SZAC_ENB_PROC" suId="1" spid="1" suEndpId="1" spEndpId="0" /> 
      </tapa.send>

   </tapa.steps>

</tapa.command>

#
#       Test:    tapa.dummyproxy.deinit
#
#       Desc:    Do shutdown
#
#       Notes:   None
#
#       Module:  None
#
#       File:    szac_cmd1.xml
#
#
<tapa.command id="tapa.sz.dummyproxy.deinit">

   # Document subsection
   <tapa.doc>

      <tapa.desc>
         "Shutdown dummy proxy"
      </tapa.desc>

      <tapa.longdesc/>

   </tapa.doc>

   # Steps subsection
   <tapa.steps>
      # Do shutdown
      <tapa.delay delay="1"/>

      <tapa.send>
         <tapa.lsz.cntrl.shutdown/>
      </tapa.send>

      # Expect Term Req for both MME and eNB peers
      <tapa.expect>
         <tapa.sct.termreq procId="SZAC_ENB_PROC" spId="1" assocIdx="1"  assocIdType="2"/> 
      </tapa.expect>
  
      <tapa.expect>
         <tapa.sct.termreq procId="SZAC_ENB_PROC" spId="0" spAssocId="2" assocIdx="0" assocIdType="2"/> 
      </tapa.expect>

      # Expect EndpClose Req to dummy SCTP
      <tapa.expect>
         <tapa.sct.endpclosereq procId="SZAC_ENB_PROC" spId="0" suEndpId="0" endpIdType="2"/> 
      </tapa.expect>

       # Expect EndpClose Req to dummy SCTP
      <tapa.expect>
         <tapa.sct.endpclosereq procId="SZAC_ENB_PROC" spId="1" suEndpId="0" endpIdType="2"/> 
      </tapa.expect>
  
      # Do shutdown
      <tapa.expect>
         <tapa.lsz.cntrlcfm/>
      </tapa.expect>

      <tapa.delay delay="1"/>

   </tapa.steps>

</tapa.command>


