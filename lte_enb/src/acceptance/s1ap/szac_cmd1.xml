         <tapa.lhi.cntrlcfm procId="SZAC_MME_PROC"/>
      </tapa.expect>

      #Add delay between msg */
      #<tapa.delay delay="1"/>
      
   </tapa.steps>

</tapa.command>

#
#
#       Command: tapa.sb.init
#
#       Desc:    Command to SCTP gen and ssap cfg
#
#       Notes:   None
#              
#       Module:  S1AP
#
#       File:    xx_tc1.xml
#
#
<tapa.command id="tapa.sb.init">

   # Document subsection
   <tapa.doc>

      <tapa.desc>
         "Do SCTP initialisation"
      </tapa.desc>

      <tapa.longdesc/>
         
   </tapa.doc>

   # Test Steps subsection
   <tapa.steps>

      # Send Lsb general configuration request
      <tapa.send flag="SZ_SCTP">
        <tapa.lsb.cfg.gen/>
      </tapa.send>

      # expect Lsb general configuration confirm
      <tapa.expect flag="SZ_SCTP">
        <tapa.lsb.cfgcfm/>
      </tapa.expect>

      #configure  LOWER TSAP of SCTP 
      <tapa.send flag="SZ_SCTP">
        <tapa.lsb.cfg.tsap tSapId="0" spId="0" nmbAddrLst="1"/>
      </tapa.send>

      #LSB TSAP configure confirm
      <tapa.expect flag="SZ_SCTP">
        <tapa.lsb.cfgcfm/>
      </tapa.expect>

      #configure UPPER SSAP of sctp
      <tapa.send flag="SZ_SCTP">
        <tapa.lsb.cfg.sctsap sSapId="0"/>
      </tapa.send>
   
      #LSB SSAP configure confirm
      <tapa.expect flag="SZ_SCTP">
        <tapa.lsb.cfgcfm/>
      </tapa.expect>

      #configure UPPER SSAP of sctp
      <tapa.send flag="SZ_SCTP">
        <tapa.lsb.cfg.sctsap sSapId="1"/>
      </tapa.send>
   
      #LSB SSAP configure confirm
      <tapa.expect flag="SZ_SCTP">
        <tapa.lsb.cfgcfm/>
      </tapa.expect>

      <tapa.delay delay="1"/>

      #bind enable SCTP TSAP 
      <tapa.send flag="SZ_SCTP">
        <tapa.lsb.cntrl.tsap tSapId="0"/>
      </tapa.send>
  
      #expect LSB CNTRL Cfm 
      <tapa.expect flag="SZ_SCTP">
        <tapa.lsb.cntrlcfm/>
      </tapa.expect>

      ########## Integration Testing ############
      # Send Lsb general configuration request
      <tapa.send flag="SZAC_INTG_TEST">
        <tapa.lsb.cfg.gen procId="SZAC_MME_PROC"/>
      </tapa.send>

      # expect Lsb general configuration confirm
      <tapa.expect flag="SZAC_INTG_TEST">
        <tapa.lsb.cfgcfm procId="SZAC_MME_PROC"/>
      </tapa.expect>

      #configure  LOWER TSAP of SCTP 
      <tapa.send flag="SZAC_INTG_TEST">
        <tapa.lsb.cfg.tsap tSapId="0" spId="0" nmbAddrLst="1" procId="SZAC_MME_PROC"/>
      </tapa.send>

      #LSB TSAP configure confirm
      <tapa.expect flag="SZAC_INTG_TEST">
        <tapa.lsb.cfgcfm procId="SZAC_MME_PROC"/>
      </tapa.expect>

      #configure UPPER SSAP of sctp
      <tapa.send flag="SZAC_INTG_TEST">
        <tapa.lsb.cfg.sctsap sSapId="1" procId="SZAC_MME_PROC"/>
      </tapa.send>
   
      #LSB SSAP configure confirm
      <tapa.expect flag="SZAC_INTG_TEST">
        <tapa.lsb.cfgcfm procId="SZAC_MME_PROC"/>
      </tapa.expect>

      <tapa.delay delay="1"/>

      #bind enable SCTP TSAP 
      <tapa.send flag="SZAC_INTG_TEST">
        <tapa.lsb.cntrl.tsap tSapId="0" procId="SZAC_MME_PROC"/>
      </tapa.send>
  
      #expect LSB CNTRL Cfm 
      <tapa.expect flag="SZAC_INTG_TEST">
        <tapa.lsb.cntrlcfm procId="SZAC_MME_PROC"/>
      </tapa.expect>
   </tapa.steps>

</tapa.command>

