<tapa.command id="tapa.hi.init">

   # Document subsection
   <tapa.doc>

      <tapa.desc>
         "Do TUCL initialisation"
      </tapa.desc>

      <tapa.longdesc/>

   </tapa.doc>

   # Steps subsection
   <tapa.steps>

      #configure the TUCL
      # Send General Configuration Request
      <tapa.send>
         <tapa.lhi.cfg.gen/>
      </tapa.send>

      # Expect General Configuration Confirm
      <tapa.expect>
         <tapa.lhi.cfgcfm/>
      </tapa.expect>

      # Send TSAP Configuration Request
      <tapa.send>
         <tapa.lhi.cfg.tsap tSapId="0"/>
      </tapa.send>
      
      # Expect TSAP Configuration Confirm
      <tapa.expect>
         <tapa.lhi.cfgcfm/>
      </tapa.expect>

      # Send TSAP Configuration Request
      <tapa.send flag="CZAC_LKSCTP">
         <tapa.lhi.cfg.tsap tSapId="2"/>
      </tapa.send>
      
      # Expect TSAP Configuration Confirm
      <tapa.expect flag="CZAC_LKSCTP">
         <tapa.lhi.cfgcfm/>
      </tapa.expect>

# Send TSAP Configuration Request
      <tapa.send flag="CZAC_LKSCTP">
         <tapa.lhi.cfg.tsap tSapId="3"/>
      </tapa.send>
      
      # Expect TSAP Configuration Confirm
      <tapa.expect flag="CZAC_LKSCTP">
         <tapa.lhi.cfgcfm/>
      </tapa.expect>

   </tapa.steps>

</tapa.command>

