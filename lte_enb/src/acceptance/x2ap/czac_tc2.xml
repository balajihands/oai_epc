<tapa.testcase id="2.14">

   # Document subsection
   <tapa.doc>

      <tapa.rfc id="None" section="None"/>
      <tapa.module id="x2ap"/>

      <tapa.desc>
         "Verify that X2Ap will return local error indication if User sends 
         any message other than RESET RSP, after it receives the RESET REQUEST 
         message. 
          Send RESET REQ from ENB1. Try to send CONFIG UPDATE REQUEST message
          from ENB2, after it receives RESET REQUEST. X2AP at ENB2 will send error indication
          to user. Try to send X2 SETUP REQUEST/RESET REQ message also for which same error indication
          will be sent. 
          ENB2 will send the RESET RSP message. RESET Will be successfull at ENB1 also."


      </tapa.desc>

      <tapa.longdesc/>

   </tapa.doc>

   # Require features and flags subsection
   <tapa.require>

      <tapa.ifdef>
         <tapa.flag id="CZAC_INTSCTP" />  
      </tapa.ifdef>

      <tapa.ifndef/>
      <tapa.mode mode="single"/>

   </tapa.require>

   # Configuration subsection
   <tapa.cfg>
      <tapa.hi.init/>
      <tapa.sb.init/>
      <tapa.cz.enb1.init/>
      <tapa.cz.enb2.init/>
      <tapa.cz.x2.init/>

   </tapa.cfg>

   # Test Steps subsection
   <tapa.test>

         
     # Send an RESET REQUEST message from eNB1 to ENB2
      <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_RESET" 
                         procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="0"/>
      </tapa.send>

       <tapa.delay delay="2" />
     #Expect this RESET REQUEST at target eNodeB
     <tapa.expect>
        <tapa.czt.gpind procId="CZAC_LCZ_ENB2_PRC" peerId="1"
           eventType="CZT_MSG_RESET" />
     </tapa.expect>
     
      #ENB2 sends the CONFIG UPD REQUEST at target eNodeB
       <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_CFG_UPD_REQ"
            procId="CZAC_LCZ_ENB2_PRC" peerId="1" spId="0" />
      </tapa.send>

     
     #Expect this ERROR INDICATION from ENB1 for the CONFIG UPD ACK
     <tapa.expect>
        <tapa.czt.errind procId="CZAC_LCZ_ENB2_PRC" peerId="1"
                         errType="CZT_ERRTYPE_INTERNAL"
                         errCause="CZT_ERRCAUSE_INV_STATE"
                         msgType="CZT_MSG_CFG_UPD_REQ"/>
     </tapa.expect>

     #ENB2 sends the X2 RESET REQUEST at target eNodeB
       <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_RESET"
            procId="CZAC_LCZ_ENB2_PRC" peerId="1" spId="0" />
      </tapa.send>
     #Expect this ERROR INDICATION from ENB1 for the CONFIG UPD ACK
     <tapa.expect>
        <tapa.czt.errind procId="CZAC_LCZ_ENB2_PRC" peerId="1"
                         errType="CZT_ERRTYPE_INTERNAL"
                         errCause="CZT_ERRCAUSE_INV_STATE"
                         msgType="CZT_MSG_RESET"/>
     </tapa.expect>

     #ENB2 sends the X2 SETUP REQUEST at target eNodeB
       <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_X2_SETUP"
            procId="CZAC_LCZ_ENB2_PRC" peerId="1" spId="0" />
      </tapa.send>
     #Expect this ERROR INDICATION from ENB1 for the CONFIG UPD ACK
     <tapa.expect>
        <tapa.czt.errind procId="CZAC_LCZ_ENB2_PRC" peerId="1"
                         errType="CZT_ERRTYPE_INTERNAL"
                         errCause="CZT_ERRCAUSE_INV_STATE"
                         msgType="CZT_MSG_X2_SETUP"/>
     </tapa.expect>
         
     #ENB2 sends the RESET RSP at target eNodeB
       <tapa.send>
         <tapa.czt.gprsp eventType="CZT_MSG_RESET_RSP"
            procId="CZAC_LCZ_ENB2_PRC" peerId="1" spId="0" />
      </tapa.send>
      
     #Expect this RESET RSP at target eNodeB
     <tapa.expect>
         <tapa.czt.gpcfm eventType="CZT_MSG_RESET_RSP"
            procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="0" />
     </tapa.expect>
    
     

   </tapa.test>

   # Clean subsection
   <tapa.clean/>

   # Shutdown subsection
   <tapa.shutdown>

      # x2ap cleanup operations
       <tapa.cz.enb2.deinit/>
       <tapa.cz.enb1.deinit/>
      <tapa.sb.deinit/>
      <tapa.hi.deinit/>

   </tapa.shutdown>

</tapa.testcase>

