<tapa.testcase id="2.53">

   # Document subsection
   <tapa.doc>

      <tapa.rfc id="None" section="None"/>
      <tapa.module id="x2ap"/>

      <tapa.desc>
         "Successful ENB Setup procedure.
         eNodeB2 sends another Setup Request to eNodeB1. eNodeB1 sends
         a Setup Response. The message is filled wrong. Encode of Setup
         fails, hence TPM fails to send the
         message and gives an error indication to the user."
      </tapa.desc>

      <tapa.longdesc/>

   </tapa.doc>

   # Require features and flags subsection
   <tapa.require>

      <tapa.ifdef>
         <tapa.flag id="CZAC_INTSCTP" />  
      </tapa.ifdef>

      <tapa.ifndef/>
      <tapa.mode mode="single"/>

   </tapa.require>

   # Configuration subsection
   <tapa.cfg>
      <tapa.hi.init/>
      <tapa.sb.init/>
      <tapa.cz.enb1.init/>
      <tapa.cz.enb2.init/>
      <tapa.cz.x2.init/>

   </tapa.cfg>

   # Test Steps subsection
   <tapa.test>

      # eNB2 -- Reset Request ---> eNB1
      # Fill the message erroneously to fail encode
      <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_X2_SETUP"
            procId="CZAC_LCZ_ENB2_PRC"
            peerId="1" spId="0" />
      </tapa.send>

      #Expect this SETUP REQUEST at target eNodeB
     <tapa.expect>
        <tapa.czt.gpind procId="CZAC_LCZ_ENB1_PRC" peerId="2"
           eventType="CZT_MSG_X2_SETUP" />
     </tapa.expect>
    
     #ENB2 sends the SETUP RSP at target eNodeB
       <tapa.send>
         <tapa.czt.gprsp eventType="CZT_MSG_X2_SETUP_RSP"
            procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="0"
            encodeFail="TRUE"/>
      </tapa.send>
      

      #Expect error Indication, Send failed due to encode error
      <tapa.expect>
        <tapa.czt.errind errType="CZT_ERRTYPE_MSG_SEND_FAILED" 
                         errCause="CZT_ERRCAUSE_ENC_FAILED" 
                         msgType="CZT_MSG_X2_SETUP_RSP" 
                         procId="CZAC_LCZ_ENB1_PRC"
                         peerId="2" />
      </tapa.expect>

      <tapa.delay delay="15" />
      
   </tapa.test>

   # Clean subsection
   <tapa.clean/>

   # Shutdown subsection
   <tapa.shutdown>

      # x2ap cleanup operations
       <tapa.cz.enb2.deinit/>
       <tapa.cz.enb1.deinit/>
      <tapa.sb.deinit/>
      <tapa.hi.deinit/>

   </tapa.shutdown>

</tapa.testcase>

