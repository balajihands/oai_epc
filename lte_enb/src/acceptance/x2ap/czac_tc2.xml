<tapa.testcase id="2.45">

   # Document subsection
   <tapa.doc>

      <tapa.rfc id="None" section="None"/>
      <tapa.module id="x2ap"/>

      <tapa.desc>
         "Successful ENB Config update procedure.
         unbind the upper SAP at eNB2.
         Send an CONFIG UPDATE REQUEST message from eNB1 to ENB2
         The eNB2 fails to indicate the Config Update to the user
         since the SSap is disabled.
         It sends a Config Update failure to eNB1.
         When The failure is received at eNB1, it starts the Config update
         wait timer found in the failure response.
         Expect this CONFIG UPDATE failure at target eNodeB1."


      </tapa.desc>

      <tapa.longdesc/>

   </tapa.doc>

   # Require features and flags subsection
   <tapa.require>

      <tapa.ifdef>
         <tapa.flag id="CZAC_INTSCTP" />  
      </tapa.ifdef>

      <tapa.ifndef/>
      <tapa.mode mode="single"/>

   </tapa.require>

   # Configuration subsection
   <tapa.cfg>
      <tapa.hi.init/>
      <tapa.sb.init/>
      <tapa.cz.enb1.init/>
      <tapa.cz.enb2.init/>
      <tapa.cz.x2.init/>

   </tapa.cfg>

   # Test Steps subsection
   <tapa.test>

      # unbind the upper SAP at eNB2.
      <tapa.send>
         <tapa.czt.ubndreq procId="CZAC_LCZ_ENB2_PRC" spId="0" suId="0"/>
      </tapa.send>

      # Send an CONFIG UPDATE REQUEST message from eNB1 to ENB2
      <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_CFG_UPD_REQ" procId="CZAC_LCZ_ENB1_PRC" peerId="2"
            spId="0"/>
      </tapa.send>

      # The eNB2 fails to indicate the Config Update to the user
      # since the SSap is disabled.
      # It sends a Config Update failure to eNB1.
      # When The failure is received at eNB1, it starts the Config update
      # wait timer found in the failure response.
      <tapa.expect>
        <tapa.czt.staind peerId="2" staType="CZT_STA_TYPE_TMR_START"
                         procId="CZAC_LCZ_ENB1_PRC" 
                         status="CZT_STATUS_SETUPREQ_TMR_STARTED"
                         reason="CZT_REASON_NOT_APPL"/>
     </tapa.expect>

     #Expect this CONFIG UPDATE failure at target eNodeB1
     <tapa.expect>
        <tapa.czt.gpcfm procId="CZAC_LCZ_ENB1_PRC" peerId="2"
                      causeType="CAUSE_PROTOCOL" 
                           causeVal="3"
           eventType="CZT_MSG_CFG_UPD_FAIL" />
     </tapa.expect>
    
     <tapa.delay delay="10" />
      
   </tapa.test>

   # Clean subsection
   <tapa.clean/>

   # Shutdown subsection
   <tapa.shutdown>

      # x2ap cleanup operations
       <tapa.cz.enb2.deinit/>
       <tapa.cz.enb1.deinit/>
      <tapa.sb.deinit/>
      <tapa.hi.deinit/>

   </tapa.shutdown>

</tapa.testcase>


