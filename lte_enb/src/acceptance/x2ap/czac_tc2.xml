<tapa.testcase id="2.40">

   # Document subsection
   <tapa.doc>

      <tapa.rfc id="None" section="None"/>
      <tapa.module id="x2ap"/>

      <tapa.desc>
      "Send an X2 SETUP REQUEST message from eNB to ENB1
      Expect this X2 Setup at target eNodeB
      ENB2 sends the X2 Setup fail at target eNodeB
      Expect STAIND for TimeToWait timer start
      unbind the upper SAP at eNB1.
      The Time to wait timer expires at eNB1.
      The X2AP fails to give this indication to the user
      since the Ssap is unbound at eNB1.
      Send an X2 SETUP REQUEST message from eNB2 to ENB1
      The X2AP at eNB1 fails to indicate this to the user
      since the SSap is unbound.
      Hence stack responds with Setup Failure message.
      The X2AP at eNB2 receives the Setup Failure message.
      It starts the Time to wait timer.
      Expect this X2 Setup Fail at target eNodeB2."
      </tapa.desc>
      <tapa.longdesc/>

   </tapa.doc>

   # Require features and flags subsection
   <tapa.require>

      <tapa.ifdef>
         <tapa.flag id="CZAC_INTSCTP" />  
      </tapa.ifdef>

      <tapa.ifndef>
       <tapa.flag id="CZAC_LKSCTP"/>
      </tapa.ifndef>
      <tapa.mode mode="single"/>

   </tapa.require>

   # Configuration subsection
   <tapa.cfg>
      <tapa.hi.init/>
      <tapa.sb.init/>
      <tapa.cz.enb1.init/>
      <tapa.cz.enb2.init/>


   </tapa.cfg>

   # Test Steps subsection
   <tapa.test>

      # Send an X2 SETUP REQUEST message from eNB to ENB1
      <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_X2_SETUP" procId="CZAC_LCZ_ENB1_PRC"
            peerId="2" spId="0"/>
      </tapa.send>
      <tapa.expset>
         <tapa.lcz.staInd category="LCM_CATEGORY_PROTOCOL"
            event = "LCZ_EVENT_LI_COMM_UP"
            cause = "LCZ_CAUSE_SCT_NOT_APPL"
            type   =  "LCZ_USTA_DGNVAL_PEER"
            typeId =  "1"      
            procId="CZAC_LCZ_ENB2_PRC" />
#         <tapa.czt.staind peerId="1" staType="CZT_STA_TYPE_ASSOC"
#            procId="CZAC_LCZ_ENB2_PRC" 
#            status="CZT_STATUS_ASSOC_UP"
#            reason="CZT_REASON_RCVD_COMM_UP"/>
         <tapa.lcz.staInd category="LCM_CATEGORY_PROTOCOL"
            event = "LCZ_EVENT_LI_COMM_UP"
            cause = "LCZ_CAUSE_SCT_NOT_APPL"
            type   =  "LCZ_USTA_DGNVAL_PEER"
            typeId =  "2"      
            procId="CZAC_LCZ_ENB1_PRC" />
#         <tapa.czt.staind peerId="2" staType="CZT_STA_TYPE_ASSOC"
#            procId="CZAC_LCZ_ENB1_PRC" 
#            status="CZT_STATUS_ASSOC_UP"
#            reason="CZT_REASON_RCVD_COMM_UP"/>
      </tapa.expset>

      <tapa.delay delay="2" />
      #Expect this X2 Setup at target eNodeB
      <tapa.expect>
         <tapa.czt.gpind procId="CZAC_LCZ_ENB2_PRC" peerId="1"
            eventType="CZT_MSG_X2_SETUP" />
      </tapa.expect>

      #ENB2 sends the X2 Setup fail at target eNodeB
      <tapa.send>
         <tapa.czt.gprsp eventType="CZT_MSG_X2_SETUP_FAIL"
            procId="CZAC_LCZ_ENB2_PRC" peerId="1" spId="0" timeToWait="2"/>
      </tapa.send>

      #Expect STAIND for TimeToWait timer start
      <tapa.expset>
         <tapa.czt.staind peerId="2" staType="CZT_STA_TYPE_TMR_START"
            procId="CZAC_LCZ_ENB1_PRC" 
            status="CZT_STATUS_SETUPREQ_TMR_STARTED"
            reason="CZT_REASON_NOT_APPL"/>
         <tapa.czt.gpcfm procId="CZAC_LCZ_ENB1_PRC" peerId="2"
            eventType="CZT_MSG_X2_SETUP_FAIL" />
      </tapa.expset>

      # unbind the upper SAP at eNB1.
      <tapa.send>
         <tapa.czt.ubndreq procId="CZAC_LCZ_ENB1_PRC" spId="0" suId="0"/>
      </tapa.send>

      <tapa.delay delay="100" />

      # The Time to wait timer expires at eNB1.
      # The X2AP fails to give this indication to the user
      # since the Ssap is unbound.
      <tapa.expect>
         <tapa.lcz.staInd peerId="2" event="LCZ_EVENT_UI_MSG_PEER"
            procId="CZAC_LCZ_ENB1_PRC"
            cause="LCZ_CAUSE_NOT_BIND"
            category="LCM_CATEGORY_INTERFACE"/>
      </tapa.expect>

      # Send an X2 SETUP REQUEST message from eNB2 to ENB1
      <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_X2_SETUP" 
            procId="CZAC_LCZ_ENB2_PRC" peerId="1" spId="0"/>
      </tapa.send>

      # The X2AP at eNB1 fails to indicate this to the user
      # since the SSap is unbound.
      # Hence stack responds with Setup Failure message.
      <tapa.expect>
         <tapa.lcz.staInd peerId="2" event="LCZ_EVENT_UI_MSG_PEER"
            procId="CZAC_LCZ_ENB1_PRC"
            cause="LCZ_CAUSE_NOT_BIND"
            category="LCM_CATEGORY_INTERFACE"/>
      </tapa.expect>

      # The X2AP at eNB2 receives the Setup Failure message.
      # It starts the Time to wait timer.
      <tapa.expect>
         <tapa.czt.staind peerId="1" staType="CZT_STA_TYPE_TMR_START"
            procId="CZAC_LCZ_ENB2_PRC" 
            status="CZT_STATUS_SETUPREQ_TMR_STARTED"
            reason="CZT_REASON_NOT_APPL"/>
      </tapa.expect>

      #Expect this X2 Setup Fail at target eNodeB2
      <tapa.expect>
         <tapa.czt.gpcfm procId="CZAC_LCZ_ENB2_PRC" peerId="1"
            causeType="CAUSE_MISC" 
            causeVal="2"
            eventType="CZT_MSG_X2_SETUP_FAIL" />
      </tapa.expect>

   </tapa.test>

   # Clean subsection
   <tapa.clean/>

   # Shutdown subsection
   <tapa.shutdown>

      # x2ap cleanup operations
       <tapa.cz.enb2.deinit/>
       <tapa.cz.enb1.deinit/>
      <tapa.sb.deinit/>
      <tapa.hi.deinit/>

   </tapa.shutdown>

</tapa.testcase>

