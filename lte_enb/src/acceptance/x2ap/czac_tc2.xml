<tapa.testcase id="2.35">

   # Document subsection
   <tapa.doc>

      <tapa.rfc id="None" section="None"/>
      <tapa.module id="x2ap"/>

      <tapa.desc>
         "Successful X2Setup Req. Send a Setup Request while the wait 
         timer is on for eNB Config Update. This timer is stopped before
         sending the Setup Request. This is verified with a delay at the end."
      </tapa.desc>

      <tapa.longdesc/>

   </tapa.doc>

   # Require features and flags subsection
   <tapa.require>

      <tapa.ifdef>
         <tapa.flag id="CZAC_INTSCTP" />  
      </tapa.ifdef>

      <tapa.ifndef>
       <tapa.flag id="CZAC_LKSCTP"/>
      </tapa.ifndef>
      <tapa.mode mode="single"/>

   </tapa.require>

   # Configuration subsection
   <tapa.cfg>
      <tapa.hi.init/>
      <tapa.sb.init/>
      <tapa.cz.enb1.init/>
      <tapa.cz.enb2.init/>


   </tapa.cfg>

   # Test Steps subsection
   <tapa.test>

      # Send an X2 SETUP REQUEST message from eNB1 to eNB2
      <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_X2_SETUP" procId="CZAC_LCZ_ENB1_PRC"
            peerId="2" spId="0"/>
      </tapa.send>
      <tapa.expset>
         <tapa.lcz.staInd category="LCM_CATEGORY_PROTOCOL"
            event = "LCZ_EVENT_LI_COMM_UP"
            cause = "LCZ_CAUSE_SCT_NOT_APPL"
            type   =  "LCZ_USTA_DGNVAL_PEER"
            typeId =  "1"      
            procId="CZAC_LCZ_ENB2_PRC" />
#         <tapa.czt.staind peerId="1" staType="CZT_STA_TYPE_ASSOC"
#            procId="CZAC_LCZ_ENB2_PRC" 
#            status="CZT_STATUS_ASSOC_UP"
#            reason="CZT_REASON_RCVD_COMM_UP"/>
         <tapa.lcz.staInd category="LCM_CATEGORY_PROTOCOL"
            event = "LCZ_EVENT_LI_COMM_UP"
            cause = "LCZ_CAUSE_SCT_NOT_APPL"
            type   =  "LCZ_USTA_DGNVAL_PEER"
            typeId =  "2"      
            procId="CZAC_LCZ_ENB1_PRC" />
#         <tapa.czt.staind peerId="2" staType="CZT_STA_TYPE_ASSOC"
#            procId="CZAC_LCZ_ENB1_PRC" 
#            status="CZT_STATUS_ASSOC_UP"
#            reason="CZT_REASON_RCVD_COMM_UP"/>
      </tapa.expset>

      #Expect this X2 Setup at target eNodeB
      <tapa.expect>
         <tapa.czt.gpind procId="CZAC_LCZ_ENB2_PRC" peerId="1"
            eventType="CZT_MSG_X2_SETUP" />
      </tapa.expect>

      <tapa.delay delay="2" />

      # eNB2 sends a Setup Response
      <tapa.send>
         <tapa.czt.gprsp eventType="CZT_MSG_X2_SETUP_RSP"
            procId="CZAC_LCZ_ENB2_PRC" peerId="1" spId="0"/>
      </tapa.send>

      # eNB1 receives the Setup Response
      <tapa.expect>
         <tapa.czt.gpcfm procId="CZAC_LCZ_ENB1_PRC" peerId="2"
            eventType="CZT_MSG_X2_SETUP_RSP" />
      </tapa.expect>

      # eNB1 sends a Config Update message to eNB2
      <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_CFG_UPD_REQ" 
            procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="0"/>
      </tapa.send>

      <tapa.delay delay="2" />
      #Expect this CONFIG UPDATE at target eNodeB2
      <tapa.expect>
         <tapa.czt.gpind procId="CZAC_LCZ_ENB2_PRC" peerId="1"
            eventType="CZT_MSG_CFG_UPD_REQ" />
      </tapa.expect>

      #ENB2 sends the CONFIG UPDATE FAIL at target eNodeB
      <tapa.send>
         <tapa.czt.gprsp eventType="CZT_MSG_CFG_UPD_FAIL"
            procId="CZAC_LCZ_ENB2_PRC" peerId="1" spId="0" timeToWait="2"/>
      </tapa.send>

      #Expect STAIND for TimeToWait timer start
      <tapa.expset>
         <tapa.czt.staind peerId="2" staType="CZT_STA_TYPE_TMR_START"
            procId="CZAC_LCZ_ENB1_PRC" 
            status="CZT_STATUS_CFGUPD_TMR_STARTED"
            reason="CZT_REASON_NOT_APPL"/>
         <tapa.czt.gpcfm procId="CZAC_LCZ_ENB1_PRC" peerId="2"
            eventType="CZT_MSG_CFG_UPD_FAIL" />
      </tapa.expset>

      # The wait timer for Config Update is running at eNB1.
      # It sends another Setup Request to eNB2.
      # While sending the wait timer is stopped
      <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_X2_SETUP" procId="CZAC_LCZ_ENB1_PRC"
            peerId="2" spId="0"/>
      </tapa.send>

      #Expect this X2 Setup at target eNodeB
      <tapa.expect>
         <tapa.czt.gpind procId="CZAC_LCZ_ENB2_PRC" peerId="1"
            eventType="CZT_MSG_X2_SETUP" />
      </tapa.expect>

      <tapa.delay delay="2" />

      # The eNB2 responds with Setup Response to eNB1.
      <tapa.send>
         <tapa.czt.gprsp eventType="CZT_MSG_X2_SETUP_RSP"
            procId="CZAC_LCZ_ENB2_PRC" peerId="1" spId="0"/>
      </tapa.send>

      #Expect this X2 Setup at target eNodeB
      <tapa.expect>
         <tapa.czt.gpcfm procId="CZAC_LCZ_ENB1_PRC" peerId="2"
            eventType="CZT_MSG_X2_SETUP_RSP" />
      </tapa.expect>

      # The wait timer does not fire in the given delay.
      <tapa.delay delay="20" />

   </tapa.test>

   # Clean subsection
   <tapa.clean/>

   # Shutdown subsection
   <tapa.shutdown>

      # x2ap cleanup operations
       <tapa.cz.enb2.deinit/>
       <tapa.cz.enb1.deinit/>
      <tapa.sb.deinit/>
      <tapa.hi.deinit/>

   </tapa.shutdown>

</tapa.testcase>

