<tapa.testcase id="2.24">

   # Document subsection
   <tapa.doc>

      <tapa.rfc id="None" section="None"/>
      <tapa.module id="x2ap"/>

      <tapa.desc>
         "To verify the X2 setup Request in connected state.
         Both ENBs are in connected state.
         ENB1 sends the X2 SETUP REQUEST message. ENB2 SENDs X2 SETUP RSP message. 
         
        
         The final state at both the end will be CONNECTED. Try sending Config update 
         message from ENB1/ENB2. This will Be successful.

          "
          

      </tapa.desc>

      <tapa.longdesc/>

   </tapa.doc>

   # Require features and flags subsection
   <tapa.require>

      <tapa.ifdef>
         <tapa.flag id="CZAC_INTSCTP" />  
      </tapa.ifdef>

      <tapa.ifndef/>
      <tapa.mode mode="single"/>

   </tapa.require>

   # Configuration subsection
   <tapa.cfg>
      <tapa.hi.init/>
      <tapa.sb.init/>
      <tapa.cz.enb1.init/>
      <tapa.cz.enb2.init/>
      <tapa.cz.x2.init />


   </tapa.cfg>

   # Test Steps subsection
   <tapa.test>
     # Send an X2 SETUP REQ message from eNB1 to ENB2
      <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_X2_SETUP" 
                         procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="0"/>
      </tapa.send>



     #Expect the X2 SETUP REQ messages at target eNB1 and ENB2
     <tapa.expset>
        <tapa.czt.gpind procId="CZAC_LCZ_ENB2_PRC" peerId="1"
           eventType="CZT_MSG_X2_SETUP" />
     </tapa.expset>

     # Send an X2 SETUP RSP message from eNB1 to ENB2
      <tapa.send>
         <tapa.czt.gprsp eventType="CZT_MSG_X2_SETUP_RSP" 
                         procId="CZAC_LCZ_ENB2_PRC" peerId="1" spId="0"/>
      </tapa.send>



     #Expect the messages at target eNB1 and ENB2
     <tapa.expset>
        <tapa.czt.gpcfm procId="CZAC_LCZ_ENB1_PRC" peerId="2"
           eventType="CZT_MSG_X2_SETUP_RSP" />
     </tapa.expset>
      
      # Send an CONFIG UPDATE REQUEST message from eNB1 to ENB2
      <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_CFG_UPD_REQ" procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="0"/>
      </tapa.send>

