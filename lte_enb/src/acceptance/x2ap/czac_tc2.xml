<tapa.testcase id="2.16">

   # Document subsection
   <tapa.doc>

      <tapa.rfc id="None" section="None"/>
      <tapa.module id="x2ap"/>

      <tapa.desc>
         "To verify the cross over scenario during X2 Setup request.
         ENB1 sends the SETUP REQUEST message. ENB2 before it receives the 
         SETUP REQ message sends X2 SETUP REQUEST. 
         Test the following behaviour:
         ENB1---SETUP REQ----ENB2
         ENB2---SETUP REQ----ENB1
         ENB1---SETUP RSP----ENB2
         ENB2---SETUP FAIL---ENB1

         The final state at both the end will be IDLE. Try sending Config update 
         message from ENB1/ENB2. This will through error indication towards user.

          "
          

      </tapa.desc>

      <tapa.longdesc/>

   </tapa.doc>

   # Require features and flags subsection
   <tapa.require>

      <tapa.ifdef>
         <tapa.flag id="CZAC_INTSCTP" />  
      </tapa.ifdef>

      <tapa.ifndef/>
      <tapa.mode mode="single"/>

   </tapa.require>

   # Configuration subsection
   <tapa.cfg>
      <tapa.hi.init/>
      <tapa.sb.init/>
      <tapa.cz.enb1.init/>
      <tapa.cz.enb2.init/>
      <tapa.cz.x2.init/>

   </tapa.cfg>

   # Test Steps subsection
   <tapa.test>
      # Send an SETUP REQ message from eNB1 to ENB2
      <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_X2_SETUP" 
                         procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="0"/>
      </tapa.send>

     # Send an SETUP REQ message from eNB1 to ENB2
      <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_X2_SETUP" 
                         procId="CZAC_LCZ_ENB2_PRC" peerId="1" spId="0"/>
      </tapa.send>


     #Expect this SETUP REQUEST at target eNodeB
     <tapa.expset>
        <tapa.czt.gpind procId="CZAC_LCZ_ENB2_PRC" peerId="1"
           eventType="CZT_MSG_X2_SETUP" />
        <tapa.czt.gpind procId="CZAC_LCZ_ENB1_PRC" peerId="2"
           eventType="CZT_MSG_X2_SETUP" />   
     </tapa.expset>

      # Send an SETUP RESPONSE message from eNB1 to ENB2
      <tapa.send>
         <tapa.czt.gprsp eventType="CZT_MSG_X2_SETUP_RSP" 
                         procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="0"/>
      </tapa.send>
     #Expect this SETUP RSP at target eNodeB
     <tapa.expset>
        <tapa.czt.gpcfm procId="CZAC_LCZ_ENB2_PRC" peerId="1"
           eventType="CZT_MSG_X2_SETUP_RSP" />
     </tapa.expset>

      # Send an SETUP FAIL message from eNB1 to ENB2
      <tapa.send>
         <tapa.czt.gprsp eventType="CZT_MSG_X2_SETUP_FAIL" 
                         procId="CZAC_LCZ_ENB2_PRC" peerId="1" spId="0"/>
      </tapa.send>

     #Expect this SETUP FAIL at target eNodeB
     <tapa.expset>
        <tapa.czt.gpcfm procId="CZAC_LCZ_ENB1_PRC" peerId="2"
           eventType="CZT_MSG_X2_SETUP_FAIL" />   
     </tapa.expset>
    
      # Send CONFIG UPDATE message from eNB1 to ENB2
      <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_CFG_UPD_REQ" 
                         procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="0"/>
      </tapa.send>
      # Expect local error indication at User
      <tapa.expect>
         <tapa.czt.errind errType="CZT_ERRTYPE_INTERNAL" 
                         procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="0"
                         errCause="CZT_ERRCAUSE_INV_STATE"
                         msgType="CZT_MSG_CFG_UPD_REQ" />
      </tapa.expect>

   </tapa.test>

   # Clean subsection
   <tapa.clean/>

   # Shutdown subsection
   <tapa.shutdown>

      # x2ap cleanup operations
       <tapa.cz.enb2.deinit/>
       <tapa.cz.enb1.deinit/>
      <tapa.sb.deinit/>
      <tapa.hi.deinit/>

   </tapa.shutdown>

</tapa.testcase>

