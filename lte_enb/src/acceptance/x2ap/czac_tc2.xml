      </tapa.send>


     #Expect this RESET message at target eNodeB
     <tapa.expset>
        <tapa.czt.gpind procId="CZAC_LCZ_ENB2_PRC" peerId="1"
           eventType="CZT_MSG_RESET" />
        <tapa.czt.gpind procId="CZAC_LCZ_ENB1_PRC" peerId="2"
           eventType="CZT_MSG_RESET" />   
     </tapa.expset>

      # Send an RESET RESPONSE message from eNB1 to ENB2
      <tapa.send>
         <tapa.czt.gprsp eventType="CZT_MSG_RESET_RSP" 
                         procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="0"/>
      </tapa.send>
     #Expect this RESET RSP at target eNodeB
     <tapa.expset>
