<tapa.testcase id="2.26">

   # Document subsection
   <tapa.doc>

      <tapa.rfc id="None" section="None"/>
      <tapa.module id="x2ap"/>

      <tapa.desc>
         "To Test RELREQ with type= CZT_RELTYPE_RESET during a cross over scenario,
         X2 Setup was successful. ENB1 sends X2 setup Request again.
         ENB2 also sends X2 Setup Request. ENB1 sends X2 SETUP FAIL and ENB2 sends 
         X2 SETUP ACK. The ENB1 and ENB2 will be in idle state.

         X2AP user at ENB1 sends the local release request with type 
         CZT_RELTYPE_RESET. After successful RESET try to send CONFIG UPD REQUEST.
         X2AP should return error indication to user."

      </tapa.desc>

      <tapa.longdesc/>

   </tapa.doc>

   # Require features and flags subsection
   <tapa.require>

      <tapa.ifdef>
         <tapa.flag id="CZAC_INTSCTP" />  
      </tapa.ifdef>

      <tapa.ifndef>
       <tapa.flag id="CZAC_LKSCTP"/>
      </tapa.ifndef>
      <tapa.mode mode="single"/>

   </tapa.require>

   # Configuration subsection
   <tapa.cfg>
      <tapa.hi.init/>
      <tapa.sb.init/>
      <tapa.cz.enb1.init/>
      <tapa.cz.enb2.init/>


   </tapa.cfg>

   # Test Steps subsection
   <tapa.test>

      # Send an X2 SETUP REQUEST message from eNB to ENB1
      <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_X2_SETUP" procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="0"/>
      </tapa.send>
     <tapa.expset>
         <tapa.lcz.staInd category="LCM_CATEGORY_PROTOCOL"
                       event = "LCZ_EVENT_LI_COMM_UP"
                       cause = "LCZ_CAUSE_SCT_NOT_APPL"
                       type   =  "LCZ_USTA_DGNVAL_PEER"
                       typeId =  "1"      
                       procId="CZAC_LCZ_ENB2_PRC" />
#        <tapa.czt.staind peerId="1" staType="CZT_STA_TYPE_ASSOC"
#                        procId="CZAC_LCZ_ENB2_PRC" 
#                        status="CZT_STATUS_ASSOC_UP"
#                        reason="CZT_REASON_RCVD_COMM_UP"/>
         <tapa.lcz.staInd category="LCM_CATEGORY_PROTOCOL"
                       event = "LCZ_EVENT_LI_COMM_UP"
                       cause = "LCZ_CAUSE_SCT_NOT_APPL"
                       type   =  "LCZ_USTA_DGNVAL_PEER"
                       typeId =  "2"      
                       procId="CZAC_LCZ_ENB1_PRC" />
#        <tapa.czt.staind peerId="2" staType="CZT_STA_TYPE_ASSOC"
#                         procId="CZAC_LCZ_ENB1_PRC" 
#                         status="CZT_STATUS_ASSOC_UP"
#                         reason="CZT_REASON_RCVD_COMM_UP"/>
      </tapa.expset>

     <tapa.delay delay="2" />
     #Expect this X2 Setup at target eNodeB
     <tapa.expect>
        <tapa.czt.gpind procId="CZAC_LCZ_ENB2_PRC" peerId="1"
           eventType="CZT_MSG_X2_SETUP" />
     </tapa.expect>
    
#EnodeB2 did not Send SetUp Rsp. EnodeB1 should send the setUprequest Again
#     thorugh ReTxReq.ENB1 sends the setup again through setup request.
       <tapa.send>
         <tapa.czt.gpretxreq msgType="CZT_MSG_X2_SETUP"
            procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="0"/>
      </tapa.send>
     #Expect this X2 Setup at target eNodeB
     <tapa.expect>
        <tapa.czt.gpind procId="CZAC_LCZ_ENB2_PRC" peerId="1"
           eventType="CZT_MSG_X2_SETUP" />
     </tapa.expect>
     #ENB2 sends the X2 Setup Fail at target eNodeB
       <tapa.send>
         <tapa.czt.gprsp eventType="CZT_MSG_X2_SETUP_FAIL"
            procId="CZAC_LCZ_ENB2_PRC" peerId="1" spId="0"/>
      </tapa.send>
      
     #Expect this X2 Setup Fail at target eNodeB
     <tapa.expect>
        <tapa.czt.gpcfm procId="CZAC_LCZ_ENB1_PRC" peerId="2"
           eventType="CZT_MSG_X2_SETUP_FAIL" />
     </tapa.expect>
     #ENB1 sends the Local Release with type CZT_RELTYPE_RESET
     <tapa.send>
         <tapa.czt.relreq relType="CZT_RELTYPE_RESET"
            procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="0"/>
      </tapa.send>
      #Expect STAIND at ENB2
     <tapa.expset>
       <tapa.czt.relcfm procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="0"
                       relType="CZT_RELTYPE_RESET"
                       status="CZT_PRIM_OK" reason="CZT_REASON_NOT_APPL" />
       #EXPEct Alram Too At both ends. Add during testing.
      </tapa.expset> 

   </tapa.test>

   # Clean subsection
   <tapa.clean/>

   # Shutdown subsection
   <tapa.shutdown>

      # x2ap cleanup operations
       <tapa.cz.enb2.deinit/>
       <tapa.cz.enb1.deinit/>
      <tapa.sb.deinit/>
      <tapa.hi.deinit/>

   </tapa.shutdown>

</tapa.testcase>

