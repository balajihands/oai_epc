<tapa.testcase id="2.91">

   # Document subsection
   <tapa.doc>

      <tapa.rfc id="None" section="None"/>
      <tapa.module id="x2ap"/>

      <tapa.desc>
         " To Test that ENB2 successfully accepts a connection from a peer ENB which
         is not configured previously through PeerCntrlReq."

      </tapa.desc>

      <tapa.longdesc/>

   </tapa.doc>

   # Require features and flags subsection
   <tapa.require>

      <tapa.ifdef>
         <tapa.flag id="CZAC_INTSCTP" />  
      </tapa.ifdef>

      <tapa.ifndef>
       <tapa.flag id="CZAC_LKSCTP"/>
      </tapa.ifndef>
      <tapa.mode mode="single"/>

   </tapa.require>

   # Configuration subsection
   <tapa.cfg>
      <tapa.hi.init/>
      <tapa.sb.init/>
      <tapa.cz.enb1.init/>
      <tapa.cz.dynamenb2.init dynamPeerFlag="TRUE"/>


   </tapa.cfg>

   # Test Steps subsection
   <tapa.test>

      # Send an X2 SETUP REQUEST message from eNB1 to eNB2
      <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_X2_SETUP" procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="0"/>
      </tapa.send>

       # Expect assoc indication at eNB2
      <tapa.expect>
         <tapa.czt.assocind procId="CZAC_LCZ_ENB2_PRC" peerId="0" suId="0"/>
      </tapa.expect>

     <tapa.delay delay="3" />
      # Send an assoc response from eNB2 for dynamic peer 
      <tapa.send>
         <tapa.czt.assocrsp procId="CZAC_LCZ_ENB2_PRC" peerId="1" 
                            spId="0" addressType="4" 
                            priDstAddr="127.0.0.1" 
                            dstPort="10002" comProcStrm="1" globalStreamId="5"
                            locOutStrms="0" status="CZT_PRIM_OK" 
                            cause="CZT_STACAUSE_NOT_APPL"/>
      </tapa.send>
     <tapa.delay delay="3" />
     <tapa.expset>
          <tapa.czt.staind peerId="1" staType="CZT_STA_TYPE_PEER"
                         procId="CZAC_LCZ_ENB2_PRC" 
                         status="CZT_STATUS_DYNAM_PEERCFG_NOK"
                         reason="CZT_REASON_INV_PARAM"/> 
          <tapa.czt.staind peerId="2" staType="CZT_STA_TYPE_ASSOC"
                         procId="CZAC_LCZ_ENB1_PRC" 
                         status="CZT_STATUS_ASSOC_DOWN"
                         reason="CZT_REASON_RCVD_TERMIND"/> 

           <tapa.lcz.staInd category="LCM_CATEGORY_PROTOCOL"
                       event = "LCZ_EVENT_LI_TERM_IND"
                       cause = "LCZ_CAUSE_ASSOC_DOWN"
                       type   =  "LCZ_USTA_DGNVAL_PEER"
                       typeId =  "2"      
                       procId="CZAC_LCZ_ENB1_PRC" /> 
     </tapa.expset>

   </tapa.test>

   # Clean subsection
   <tapa.clean/>

   # Shutdown subsection
   <tapa.shutdown>

      # x2ap cleanup operations
       <tapa.cz.dynamenb2.deinit  dynamPeerFlag="TRUE"/>
       <tapa.cz.enb1.deinit/>
      <tapa.sb.deinit/>
      <tapa.hi.deinit/>

   </tapa.shutdown>

</tapa.testcase>
