<tapa.testcase id="2.15">

   # Document subsection
   <tapa.doc>

      <tapa.rfc id="None" section="None"/>
      <tapa.module id="x2ap"/>

      <tapa.desc>
         "To verify retransmission of RESET REQUEST.
         ENB1 sends the RESET request message. ENB2 doesnot send RESET RESPONSE MESSAGE.
         ENB1 will try to send again the RESET REQ through RetxReq primitive.
          "
          

      </tapa.desc>

      <tapa.longdesc/>

   </tapa.doc>

   # Require features and flags subsection
   <tapa.require>

      <tapa.ifdef>
         <tapa.flag id="CZAC_INTSCTP" />  
      </tapa.ifdef>

      <tapa.ifndef/>
      <tapa.mode mode="single"/>

   </tapa.require>

   # Configuration subsection
   <tapa.cfg>
      <tapa.hi.init/>
      <tapa.sb.init/>
      <tapa.cz.enb1.init/>
      <tapa.cz.enb2.init/>
      <tapa.cz.x2.init/>

   </tapa.cfg>

   # Test Steps subsection
   <tapa.test>

      # Send an RESET REQUEST message from eNB1 to ENB2
      <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_RESET" 
                         procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="0"/>
      </tapa.send>


     #Expect this RESET REQUEST at target eNodeB
     <tapa.expect>
        <tapa.czt.gpind procId="CZAC_LCZ_ENB2_PRC" peerId="1"
           eventType="CZT_MSG_RESET" />
     </tapa.expect>

     <tapa.delay delay="2" />
     #ENB1 sends the RESET REQUEST again at target eNodeB
       <tapa.send>
         <tapa.czt.gpretxreq peerId="2" msgType="CZT_MSG_RESET"
            procId="CZAC_LCZ_ENB1_PRC" spId="0" />
      </tapa.send>
     #Expect this RESET REQUEST at target eNodeB
     <tapa.expect>
        <tapa.czt.gpind procId="CZAC_LCZ_ENB2_PRC" peerId="1"
           eventType="CZT_MSG_RESET" />
     </tapa.expect>

     #ENB2 sends the RESET RESPONSE again at target eNodeB
       <tapa.send>
         <tapa.czt.gprsp peerId="1" eventType="CZT_MSG_RESET_RSP"
            procId="CZAC_LCZ_ENB2_PRC" spId="0" />
      </tapa.send>

      
     #Expect this RESET RSP at target eNodeB
     <tapa.expect>
        <tapa.czt.gpcfm procId="CZAC_LCZ_ENB1_PRC" peerId="2"
           eventType="CZT_MSG_RESET_RSP" />
     </tapa.expect>

   </tapa.test>

   # Clean subsection
   <tapa.clean/>

   # Shutdown subsection
   <tapa.shutdown>

      # x2ap cleanup operations
       <tapa.cz.enb2.deinit/>
       <tapa.cz.enb1.deinit/>
      <tapa.sb.deinit/>
      <tapa.hi.deinit/>

   </tapa.shutdown>

</tapa.testcase>


