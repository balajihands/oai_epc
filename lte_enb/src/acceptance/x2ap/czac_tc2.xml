      <tapa.desc>
         "ENB1 sends the config update. ENB2 sends the CFG UPD FAIL message with 
          TIME TOWAIT IE. ENB1 will  send again the CFG UPD message only after
          the timeToWait Ie expires."
#
#   S eNodeB-----CFGUPD Request---------------------------------------->T eNodeB
#   S eNodeB----CFGUPD FAIL with TIMETOWAIT----------------------------T eNodeB
#   S eNodeB----CFGUPD REQ after timeToWait Tmr expires--------------->T eNodeB
          

      </tapa.desc>

      <tapa.longdesc/>

   </tapa.doc>

   # Require features and flags subsection
   <tapa.require>

      <tapa.ifdef>
         <tapa.flag id="CZAC_INTSCTP" />  
      </tapa.ifdef>

      <tapa.ifndef/>
      <tapa.mode mode="single"/>

   </tapa.require>

   # Configuration subsection
   <tapa.cfg>
      <tapa.hi.init/>
      <tapa.sb.init/>
      <tapa.cz.enb1.init/>
      <tapa.cz.enb2.init/>
      <tapa.cz.x2.init/>

   </tapa.cfg>

   # Test Steps subsection
   <tapa.test>
      # Send an CONFIG UPDATE REQUEST message from eNB to ENB1
      <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_CFG_UPD_REQ" 
                         procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="0"/>
      </tapa.send>
     #Expect this CONFIG UPDATE at target eNodeB
     <tapa.expect>
        <tapa.czt.gpind procId="CZAC_LCZ_ENB2_PRC" peerId="1"
           eventType="CZT_MSG_CFG_UPD_REQ" />
     </tapa.expect>
    
     #ENB2 sends the CONFIG UPDATE FAIL at target eNodeB
       <tapa.send>
         <tapa.czt.gprsp eventType="CZT_MSG_CFG_UPD_FAIL"
            procId="CZAC_LCZ_ENB2_PRC" peerId="1" spId="0" timeToWait="2"/>
      </tapa.send>
     #Expect STAIND for TimeToWait timer start
     <tapa.expset>
        <tapa.czt.staind peerId="2" staType="CZT_STA_TYPE_TMR_START"
                         procId="CZAC_LCZ_ENB1_PRC" 
                         status="CZT_STATUS_CFGUPD_TMR_STARTED"
                         reason="CZT_REASON_NOT_APPL"/>
        <tapa.czt.gpcfm procId="CZAC_LCZ_ENB1_PRC" peerId="2"
           eventType="CZT_MSG_CFG_UPD_FAIL" />
     </tapa.expset>
      
      # Send an CFG UPD REQ message from eNB to ENB1 before timer expires
      <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_CFG_UPD_REQ" 
                         procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="0"/>
      </tapa.send>
      
      #Expect error Indication
      <tapa.expect>
        <tapa.czt.errind errType="CZT_ERRTYPE_INTERNAL" 
                         errCause="CZT_ERRCAUSE_WAIT_TMR_ON" 
                         msgType="CZT_MSG_CFG_UPD_REQ" 
                         procId="CZAC_LCZ_ENB1_PRC" peerId="2" />
      </tapa.expect>

     <tapa.delay delay="2" />
     #Expect STAIND After timer expires
     <tapa.expect>
        <tapa.czt.staind peerId="2" staType="CZT_STA_TYPE_TMR_EXPIRY"
                        procId="CZAC_LCZ_ENB1_PRC" 
                        status="CZT_STATUS_CFGUPD_TW_EXPIRY"
                        reason="CZT_REASON_NOT_APPL"/>
     </tapa.expect>
      # Send an CONFIG UPDATE REQUEST message from eNB to ENB1
      <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_CFG_UPD_REQ" 
                         procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="0"/>
      </tapa.send>
     #Expect this CONFIG UPDATE REQUEST at target eNodeB
     <tapa.expect>
        <tapa.czt.gpind procId="CZAC_LCZ_ENB2_PRC" peerId="1"
           eventType="CZT_MSG_CFG_UPD_REQ" />
     </tapa.expect>

      # Send an CONFIG UPDATE ACK message from eNB to ENB1
      <tapa.send>
         <tapa.czt.gprsp eventType="CZT_MSG_CFG_UPD_ACK" 
                         procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="0"/>
      </tapa.send>
     #Expect this CONFIG UPDATE ACK at target eNodeB
     <tapa.expect>
        <tapa.czt.gpcfm procId="CZAC_LCZ_ENB2_PRC" peerId="1"
           eventType="CZT_MSG_CFG_UPD_ACK" />
     </tapa.expect>

   </tapa.test>

   # Clean subsection
   <tapa.clean/>

   # Shutdown subsection
   <tapa.shutdown>

