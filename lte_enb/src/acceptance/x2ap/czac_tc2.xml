      <tapa.sb.init/>
      <tapa.cz.enb1.init/>
      <tapa.cz.enb2.init/>
      <tapa.cz.x2.init />

   </tapa.cfg>

   # Test Steps subsection
   <tapa.test>

      # Send an RESET message from eNB1 to ENB2
      <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_RESET" 
                         procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="0"/>
      </tapa.send>

     # Send an RESET message from eNB1 to ENB2
