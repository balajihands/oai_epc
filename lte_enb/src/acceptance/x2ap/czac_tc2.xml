<tapa.testcase id="2.4">

   # Document subsection
   <tapa.doc>

      <tapa.rfc id="None" section="None"/>
      <tapa.module id="x2ap"/>

      <tapa.desc>
         "ENB1 sends X2 Setup request. Association went successfully. ENB2
         receives X2 Setup Request from ENB1, but sends X2 setup fail with TimeTowait.
         VeriFy that X2Ap User sends the X2Setup Reuqest only after TimeToWait expires."

      </tapa.desc>

      <tapa.longdesc/>

   </tapa.doc>

   # Require features and flags subsection
   <tapa.require>

      <tapa.ifdef>
         <tapa.flag id="CZAC_INTSCTP" />  
      </tapa.ifdef>

      <tapa.ifndef>
       <tapa.flag id="CZAC_LKSCTP"/>
      </tapa.ifndef>
      <tapa.mode mode="single"/>

   </tapa.require>

   # Configuration subsection
   <tapa.cfg>
      <tapa.hi.init/>
      <tapa.sb.init/>
      <tapa.cz.enb1.init/>
      <tapa.cz.enb2.init/>


   </tapa.cfg>

   # Test Steps subsection
   <tapa.test>

      # Send an X2 SETUP REQUEST message from eNB to ENB1
      <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_X2_SETUP" procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="0"/>
      </tapa.send>
     <tapa.expset>
         <tapa.lcz.staInd category="LCM_CATEGORY_PROTOCOL"
                       event = "LCZ_EVENT_LI_COMM_UP"
                       cause = "LCZ_CAUSE_SCT_NOT_APPL"
                       type   =  "LCZ_USTA_DGNVAL_PEER"
                       typeId =  "1"      
                       procId="CZAC_LCZ_ENB2_PRC" />
#        <tapa.czt.staind peerId="1" staType="CZT_STA_TYPE_ASSOC"
#                        procId="CZAC_LCZ_ENB2_PRC" 
#                        status="CZT_STATUS_ASSOC_UP"
#                        reason="CZT_REASON_RCVD_COMM_UP"/>
         <tapa.lcz.staInd category="LCM_CATEGORY_PROTOCOL"
                       event = "LCZ_EVENT_LI_COMM_UP"
                       cause = "LCZ_CAUSE_SCT_NOT_APPL"
                       type   =  "LCZ_USTA_DGNVAL_PEER"
                       typeId =  "2"      
                       procId="CZAC_LCZ_ENB1_PRC" />
#        <tapa.czt.staind peerId="2" staType="CZT_STA_TYPE_ASSOC"
#                         procId="CZAC_LCZ_ENB1_PRC" 
#                         status="CZT_STATUS_ASSOC_UP"
#                         reason="CZT_REASON_RCVD_COMM_UP"/>
      </tapa.expset>

     <tapa.delay delay="2" />
     #Expect this X2 Setup at target eNodeB
     <tapa.expect>
        <tapa.czt.gpind procId="CZAC_LCZ_ENB2_PRC" peerId="1"
           eventType="CZT_MSG_X2_SETUP" />
     </tapa.expect>
    
     #ENB2 sends the X2 Setup fail at target eNodeB
       <tapa.send>
         <tapa.czt.gprsp eventType="CZT_MSG_X2_SETUP_FAIL"
            procId="CZAC_LCZ_ENB2_PRC" peerId="1" spId="0" timeToWait="2"/>
      </tapa.send>
      
     #Expect STAIND for TimeToWait timer start
     <tapa.expset>
        <tapa.czt.staind peerId="2" staType="CZT_STA_TYPE_TMR_START"
                         procId="CZAC_LCZ_ENB1_PRC" 
                         status="CZT_STATUS_SETUPREQ_TMR_STARTED"
                         reason="CZT_REASON_NOT_APPL"/>
        <tapa.czt.gpcfm procId="CZAC_LCZ_ENB1_PRC" peerId="2"
           eventType="CZT_MSG_X2_SETUP_FAIL" />
     </tapa.expset>
      # Send an X2 SETUP REQUEST message from eNB to ENB1 before timer expires
      <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_X2_SETUP" procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="0"/>
      </tapa.send>
      #Expect error Indication
      <tapa.expect>
        <tapa.czt.errind errType="CZT_ERRTYPE_INTERNAL" 
                         errCause="CZT_ERRCAUSE_WAIT_TMR_ON" 
                         msgType="CZT_MSG_X2_SETUP" 
                         procId="CZAC_LCZ_ENB1_PRC"
                         peerId="2" />
      </tapa.expect>

     <tapa.delay delay="5" />
     #Expect STAIND After timer expires
     <tapa.expect>
        <tapa.czt.staind peerId="2" staType="CZT_STA_TYPE_TMR_EXPIRY"
                        procId="CZAC_LCZ_ENB1_PRC" 
                        status="CZT_STATUS_SETUPREQ_TW_EXPIRY"
                        reason="CZT_REASON_NOT_APPL"/>
     </tapa.expect>
      # Send an X2 SETUP REQUEST message from eNB to ENB1
      <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_X2_SETUP" procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="0"/>
      </tapa.send>
     #Expect this X2 Setup at target eNodeB
     <tapa.expect>
        <tapa.czt.gpind procId="CZAC_LCZ_ENB2_PRC" peerId="1"
           eventType="CZT_MSG_X2_SETUP" />
     </tapa.expect>

      # Send an X2 SETUP REQUEST message from eNB to ENB1
      <tapa.send>
         <tapa.czt.gprsp eventType="CZT_MSG_X2_SETUP_RSP" 
                         procId="CZAC_LCZ_ENB2_PRC" peerId="1" spId="0"/>
      </tapa.send>
     #Expect this X2 Setup at target eNodeB
     <tapa.expect>
        <tapa.czt.gpcfm procId="CZAC_LCZ_ENB1_PRC" peerId="2"
           eventType="CZT_MSG_X2_SETUP_RSP" />
     </tapa.expect>

   </tapa.test>

   # Clean subsection
   <tapa.clean/>

   # Shutdown subsection
   <tapa.shutdown>

      # x2ap cleanup operations
       <tapa.cz.enb2.deinit/>
       <tapa.cz.enb1.deinit/>
      <tapa.sb.deinit/>
      <tapa.hi.deinit/>

   </tapa.shutdown>

</tapa.testcase>


