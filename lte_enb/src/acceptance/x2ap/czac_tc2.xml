<tapa.testcase id="2.22">

   # Document subsection
   <tapa.doc>

      <tapa.rfc id="None" section="None"/>
      <tapa.module id="x2ap"/>

      <tapa.desc>
         "To verify the cross over scenario during X2 RESET and SETUP REQ.
         Both ENBs are in connected state.
         ENB1 sends the RESET REQUEST message. ENB2 before it receives the 
         this message sends SET UP REQUEST. 
         Test the following behaviour:
         ENB1--->RESET REQ---->ENB2
         ENB2--->SET UP REQ---->ENB1
         ENB2--->RESET RSP---->ENB1
         ENB1--->SETUP ACK---->ENB2

         The final state at both the end will be CONNECTED. Try sending Config update 
         message from ENB1/ENB2. This will Be successful.
         Need to check the code for the steps.
          "
          

      </tapa.desc>

      <tapa.longdesc/>

   </tapa.doc>

   # Require features and flags subsection
   <tapa.require>

      <tapa.ifdef>
         <tapa.flag id="CZAC_INTSCTP" />  
      </tapa.ifdef>

      <tapa.ifndef/>
      <tapa.mode mode="single"/>

   </tapa.require>

   # Configuration subsection
   <tapa.cfg>
      <tapa.hi.init/>
      <tapa.sb.init/>
      <tapa.cz.enb1.init/>
      <tapa.cz.enb2.init/>
      <tapa.cz.x2.init />

   </tapa.cfg>

   # Test Steps subsection
   <tapa.test>

      # Send an RESET message from eNB1 to ENB2
      <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_RESET" 
                         procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="0"/>
      </tapa.send>

     # Send an SETUP REQUEST message from eNB2 to ENB1
      <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_X2_SETUP" 
                         procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="0"/>
      </tapa.send>

<tapa.delay delay="5" />
     #Expect the messages at target eNB1 and ENB2
     <tapa.expset>
        <tapa.czt.gpind procId="CZAC_LCZ_ENB2_PRC" peerId="1"
           eventType="CZT_MSG_RESET" />
         <tapa.czt.errind errType="CZT_ERRTYPE_INTERNAL"
         errCause="CZT_ERRCAUSE_INV_STATE" 
         msgType="CZT_MSG_X2_SETUP_RSP" procId="CZAC_LCZ_ENB1_PRC" peerId="2"/>
     </tapa.expset>

     <tapa.delay delay="2" />

   </tapa.test>

   # Clean subsection
   <tapa.clean/>

   # Shutdown subsection
   <tapa.shutdown>

      # x2ap cleanup operations
       <tapa.cz.enb2.deinit/>
       <tapa.cz.enb1.deinit/>
      <tapa.sb.deinit/>
      <tapa.hi.deinit/>

   </tapa.shutdown>

</tapa.testcase>

