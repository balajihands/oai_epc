<tapa.testcase id="2.30">

   # Document subsection
   <tapa.doc>

      <tapa.rfc id="None" section="None"/>
      <tapa.module id="x2ap"/>

      <tapa.desc>
         " Successful dynamic peer configuration. 
         After this delete the peer through LM cntrl Req. Add the same peer again 
         through LM cntrl request. Set the dynamPeerFlag to FALSE.
         Verify that X2 setup goes successfully."

      </tapa.desc>

      <tapa.longdesc/>

   </tapa.doc>

   # Require features and flags subsection
   <tapa.require>

      <tapa.ifdef>
         <tapa.flag id="CZAC_INTSCTP" />  
      </tapa.ifdef>

      <tapa.ifndef>
       <tapa.flag id="CZAC_LKSCTP"/>
    </tapa.ifndef>
    
      <tapa.mode mode="single"/>

   </tapa.require>

   # Configuration subsection
   <tapa.cfg>
      <tapa.hi.init/>
      <tapa.sb.init/>
      <tapa.cz.enb1.init/>
      <tapa.cz.dynamenb2.init dynamPeerFlag="TRUE"/>


   </tapa.cfg>

   # Test Steps subsection
   <tapa.test>

      # Send an X2 SETUP REQUEST message from eNB to ENB1
      <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_X2_SETUP" procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="0"/>
      </tapa.send>

       # Expect assoc indication at eNB2
      <tapa.expect>
         <tapa.czt.assocind procId="CZAC_LCZ_ENB2_PRC" peerId="0" suId="0"/>
      </tapa.expect>

      # Send an assoc response from eNB2 for dynamic peer 
      <tapa.send>
         <tapa.czt.assocrsp procId="CZAC_LCZ_ENB2_PRC" peerId="1" 
                            spId="0" addressType="4" 
                            priDstAddr="127.0.0.1" 
                            dstPort="10002" comProcStrm="1" 
                            locOutStrms="5" status="CZT_PRIM_OK" 
                            cause="CZT_STACAUSE_NOT_APPL"/>
      </tapa.send>
     <tapa.delay delay="3" />
     <tapa.expset>
         <tapa.lcz.staInd category="LCM_CATEGORY_PROTOCOL"
                       event = "LCZ_EVENT_UI_PEERASSOC_RSP"
                       cause = "LCZ_CAUSE_DYNAM_PEERCFG_OK"
                       type   =  "LCZ_USTA_DGNVAL_PEER"
                       typeId =  "1"      
                       procId="CZAC_LCZ_ENB2_PRC" />
        <tapa.czt.staind peerId="1" staType="CZT_STA_TYPE_PEER"
                        procId="CZAC_LCZ_ENB2_PRC" 
                        status="CZT_STATUS_DYNAM_PEERCFG_OK"
                        reason="CZT_REASON_NOT_APPL"/>
         <tapa.lcz.staInd category="LCM_CATEGORY_PROTOCOL"
                       event = "LCZ_EVENT_LI_COMM_UP"
                       cause = "LCZ_CAUSE_SCT_NOT_APPL"
                       type   =  "LCZ_USTA_DGNVAL_PEER"
                       typeId =  "1"      
                       procId="CZAC_LCZ_ENB2_PRC" />
#        <tapa.czt.staind peerId="1" staType="CZT_STA_TYPE_ASSOC"
#                        procId="CZAC_LCZ_ENB2_PRC" 
#                        status="CZT_STATUS_ASSOC_UP"
#                        reason="CZT_REASON_RCVD_COMM_UP"/>
         <tapa.lcz.staInd category="LCM_CATEGORY_PROTOCOL"
                       event = "LCZ_EVENT_LI_COMM_UP"
                       cause = "LCZ_CAUSE_SCT_NOT_APPL"
                       type   =  "LCZ_USTA_DGNVAL_PEER"
                       typeId =  "2"      
                       procId="CZAC_LCZ_ENB1_PRC" />
#        <tapa.czt.staind peerId="2" staType="CZT_STA_TYPE_ASSOC"
#                         procId="CZAC_LCZ_ENB1_PRC" 
#                         status="CZT_STATUS_ASSOC_UP"
#                         reason="CZT_REASON_RCVD_COMM_UP"/>
      </tapa.expset>

     <tapa.delay delay="2" />
     #Expect this X2 Setup at target eNodeB
     <tapa.expect>
        <tapa.czt.gpind procId="CZAC_LCZ_ENB2_PRC" peerId="1"
           eventType="CZT_MSG_X2_SETUP" />
     </tapa.expect>
    
     #ENB2 sends the X2 Setup response at target eNodeB
       <tapa.send>
         <tapa.czt.gprsp eventType="CZT_MSG_X2_SETUP_RSP"
            procId="CZAC_LCZ_ENB2_PRC" peerId="1" spId="0"/>
      </tapa.send>
      
     #Expect this X2 Setup ACK at target eNodeB
     <tapa.expect>
        <tapa.czt.gpcfm procId="CZAC_LCZ_ENB1_PRC" peerId="2"
           eventType="CZT_MSG_X2_SETUP_RSP" />
     </tapa.expect>

###Delete the peer through cntrl req from LM
    <tapa.send>
      <tapa.lcz.cntrl.peer peerId = "1" action="ADEL" subaction="SAELMNT"/>
      </tapa.send>
      <tapa.expset>
#        <tapa.lcz.cntrlcfm />
         <tapa.lcz.staInd category="LCM_CATEGORY_PROTOCOL"
                       event = "LCZ_EVENT_LI_TERM_IND"
                       cause = "LCZ_CAUSE_ASSOC_DOWN"
                       type   =  "LCZ_USTA_DGNVAL_PEER"
                       typeId =  "2"      
                       procId="CZAC_LCZ_ENB1_PRC" />
        <tapa.czt.staind peerId="2" staType="CZT_STA_TYPE_ASSOC"
                         procId="CZAC_LCZ_ENB1_PRC" 
                         status="CZT_STATUS_ASSOC_DOWN"
                         reason="CZT_REASON_RCVD_TERMIND"/>
   </tapa.expset>                         
   </tapa.test>
   # Clean subsection
   <tapa.clean/>

   # Shutdown subsection
   <tapa.shutdown>

      # x2ap cleanup operations
       <tapa.cz.dynamenb2.deinit/>
       <tapa.cz.enb1.deinit/>
      <tapa.sb.deinit/>
      <tapa.hi.deinit/>

   </tapa.shutdown>

</tapa.testcase>
