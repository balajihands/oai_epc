<tapa.testcase id="9.1">>

   # Document subsection
   <tapa.doc>

      <tapa.rfc id="None" section="None"/>
      <tapa.module id="x2ap"/>

      <tapa.desc>
           "Verify Release Request with invalid values "
      </tapa.desc>

      <tapa.longdesc/>

   </tapa.doc>

   # Require features and flags subsection
   <tapa.require>

        <tapa.ifdef>
         <tapa.flag id="CZAC_INTSCTP" />  
      </tapa.ifdef>

      <tapa.ifndef>
       <tapa.flag id="CZAC_LKSCTP"/>
      </tapa.ifndef>
      <tapa.mode mode="single"/>

   </tapa.require>

   # Configuration subsection
   <tapa.cfg>

      <tapa.hi.init/>
      <tapa.sb.init/>
      <tapa.cz.enb1.init/>
      <tapa.cz.enb2.init/>


   </tapa.cfg>

   <tapa.test>

      # Send with wrong spID value : More then Max value 
        <tapa.send>
         <tapa.czt.relreq relType="CZT_RELTYPE_TERM"
            procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="9"/>
      </tapa.send>

       <tapa.expset>

                   <tapa.lcz.staInd peerId="2" category="LCM_CATEGORY_INTERFACE"
                          event = "LCZ_EVENT_UI_INV_EVT"
                          cause = "LCZ_CAUSE_INV_SPID"
                          type   =  "LCZ_USTA_DGNVAL_SAPID"
                          typeId =  "9"
                          procId="CZAC_LCZ_ENB1_PRC" />

                           </tapa.expset>

      # Expect no messages
      <tapa.timeout/>

      # Send with wrong spID value: Value which is not cfgd.
        <tapa.send>
         <tapa.czt.relreq relType="CZT_RELTYPE_TERM"
            procId="CZAC_LCZ_ENB1_PRC" peerId="2" spId="2"/>
      </tapa.send>

       <tapa.expset>

                   <tapa.lcz.staInd peerId="2" category="LCM_CATEGORY_INTERFACE"
                          event = "LCZ_EVENT_UI_INV_EVT"
                          cause = "LCZ_CAUSE_INVALID_SAP"
                          type   =  "LCZ_USTA_DGNVAL_SAPID"
                          typeId =  "2"
                          procId="CZAC_LCZ_ENB1_PRC" />

                           </tapa.expset>

      # Expect no messages
      <tapa.timeout/>
      
   </tapa.test>

   # Clean subsection
   <tapa.clean/>

   # Shutdown subsection
   <tapa.shutdown>
      # x2ap cleanup operations
      # x2ap cleanup operations
       <tapa.cz.enb2.deinit/>
       <tapa.cz.enb1.deinit/>
      <tapa.sb.deinit/>
      <tapa.hi.deinit/>
   
   </tapa.shutdown>

</tapa.testcase>


