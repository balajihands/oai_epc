#
#       File:    czac_tc5.xml
#
#
<tapa.testcase id="9.18">

   # Document subsection
   <tapa.doc>

      <tapa.rfc id="None" section="None"/>
      <tapa.module id="x2ap"/>

      <tapa.desc>
       "Send an X2 SETUP REQUEST message from eNB to ENB1
      Expect this X2 Setup at target eNodeB
      </tapa.desc>

      <tapa.longdesc/>

   </tapa.doc>

   # Require features and flags subsection
   <tapa.require>

      <tapa.ifdef>
         <tapa.flag id="CZAC_INTSCTP" />  
      </tapa.ifdef>

      <tapa.ifndef>
       <tapa.flag id="CZAC_LKSCTP"/>
      </tapa.ifndef>
      <tapa.mode mode="single"/>

   </tapa.require>

   # Configuration subsection
   <tapa.cfg>
      <tapa.hi.init/>
      <tapa.sb.init/>
      <tapa.cz.enb1.init/>
      <tapa.cz.enb2.init/>


   </tapa.cfg>

   # Test Steps subsection
   <tapa.test>

      # Send an X2 SETUP REQUEST message 
      <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_X2_SETUP" procId="CZAC_LCZ_ENB1_PRC" peerId="2"
            spId="0"/>
      </tapa.send>
      <tapa.expset>
         <tapa.lcz.staInd category="LCM_CATEGORY_PROTOCOL"
            event = "LCZ_EVENT_LI_COMM_UP"
            cause = "LCZ_CAUSE_SCT_NOT_APPL"
            type   =  "LCZ_USTA_DGNVAL_PEER"
            typeId =  "1"      
            procId="CZAC_LCZ_ENB2_PRC" />
         <tapa.lcz.staInd category="LCM_CATEGORY_PROTOCOL"
            event = "LCZ_EVENT_LI_COMM_UP"
            cause = "LCZ_CAUSE_SCT_NOT_APPL"
            type   =  "LCZ_USTA_DGNVAL_PEER"
            typeId =  "2"      
            procId="CZAC_LCZ_ENB1_PRC" />
      </tapa.expset>

      <tapa.delay delay="2" />
      #Expect this X2 Setup at target eNodeB
      <tapa.expect>
         <tapa.czt.gpind procId="CZAC_LCZ_ENB2_PRC" peerId="1"
            eventType="CZT_MSG_X2_SETUP" />
      </tapa.expect>

      #ENB2 sends the X2 Setup fail at target eNodeB
      <tapa.send>
         <tapa.czt.gprsp eventType="CZT_MSG_X2_SETUP_FAIL"
            procId="CZAC_LCZ_ENB2_PRC" peerId="1" spId="0" timeToWait="2"/>
      </tapa.send>

      # TimeToWait start
      <tapa.expset>
         <tapa.czt.staind peerId="2" staType="CZT_STA_TYPE_TMR_START"
            procId="CZAC_LCZ_ENB1_PRC" 
            status="CZT_STATUS_SETUPREQ_TMR_STARTED"
            reason="CZT_REASON_NOT_APPL"/>
         <tapa.czt.gpcfm procId="CZAC_LCZ_ENB1_PRC" peerId="2"
            eventType="CZT_MSG_X2_SETUP_FAIL" />
      </tapa.expset>

      # unbind the upper SAP at eNB1.
      <tapa.send>
         <tapa.czt.ubndreq procId="CZAC_LCZ_ENB1_PRC" spId="0" suId="0"/>
      </tapa.send>

      <tapa.delay delay="30" />

      #Time 2 wait expires and stack has 2 inform usr , since upper SAP disable it can't.
      <tapa.expect>
         <tapa.lcz.staInd peerId="2" event="LCZ_EVENT_UI_MSG_PEER"
            procId="CZAC_LCZ_ENB1_PRC"
            cause="LCZ_CAUSE_NOT_BIND"
            category="LCM_CATEGORY_INTERFACE"/>
      </tapa.expect>

      # Send another X2 SETUP REQUEST message 
      <tapa.send>
         <tapa.czt.gpreq eventType="CZT_MSG_X2_SETUP"
            procId="CZAC_LCZ_ENB2_PRC" peerId="1" spId="0"/>
      </tapa.send>


      #Time 2 wait expires and stack has 2 inform usr , since upper SAP disable it can't.
      <tapa.expect>
         <tapa.lcz.staInd peerId="2" event="LCZ_EVENT_UI_MSG_PEER"
            procId="CZAC_LCZ_ENB1_PRC"
            cause="LCZ_CAUSE_NOT_BIND"
            category="LCM_CATEGORY_INTERFACE"/>
      </tapa.expect>

      # unbind the upper SAP at eNB2.
      <tapa.send guardTmr="30">
         <tapa.czt.ubndreq procId="CZAC_LCZ_ENB2_PRC" spId="0" suId="0"/>
      </tapa.send>
      
      <tapa.expect>
         <tapa.lcz.staInd peerId="1" event="LCZ_EVENT_UI_MSG_PEER"
            procId="CZAC_LCZ_ENB2_PRC"
            cause="LCZ_CAUSE_NOT_BIND"
            category="LCM_CATEGORY_INTERFACE"/>
      </tapa.expect>
   

      #Time 2 wait expires and stack has 2 inform usr , since upper SAP disable it can't.
      # Hence generates a Error Indication to eNB1
      <tapa.expect>
         <tapa.lcz.staInd peerId="1" event="LCZ_EVENT_UI_MSG_PEER"
            procId="CZAC_LCZ_ENB2_PRC"
            cause="LCZ_CAUSE_NOT_BIND"
            category="LCM_CATEGORY_INTERFACE"/>
      </tapa.expect>

      # eNB1 fails to indicate the message to the user since the Ssap is
      # disabled.
      <tapa.expect>
         <tapa.lcz.staInd peerId="2" event="LCZ_EVENT_UI_MSG_PEER"
            procId="CZAC_LCZ_ENB1_PRC"
            cause="LCZ_CAUSE_NOT_BIND"
            category="LCM_CATEGORY_INTERFACE"/>
      </tapa.expect>

      <tapa.delay delay="30" />

   </tapa.test>

   # Clean subsection
   <tapa.clean/>

   # Shutdown subsection
   <tapa.shutdown>

      # x2ap cleanup operations
      <tapa.cz.enb2.deinit/>
      <tapa.cz.enb1.deinit/>
      <tapa.sb.deinit/>
      <tapa.hi.deinit/>

   </tapa.shutdown>

</tapa.testcase>

