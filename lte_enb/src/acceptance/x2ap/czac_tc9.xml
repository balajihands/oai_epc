#
#
#       Test:    9.9
#
#       Desc:   To verify the Dynamic Peer Response with wrong Values 
#
#       Notes:   None
#              
#       Module:  
#
#       File:    czac_tc9.xml
#
#
<tapa.testcase id="9.9">

   # Document subsection
   <tapa.doc>

      <tapa.rfc id="None" section="None"/>
      <tapa.module id="x2ap"/>

      <tapa.desc>
         " To verify the Dynamic Peer Response with wrong Values"

      </tapa.desc>

      <tapa.longdesc/>

   </tapa.doc>

   # Require features and flags subsection
   <tapa.require>

      <tapa.ifdef>
         <tapa.flag id="CZAC_INTSCTP" />  
      </tapa.ifdef>

      <tapa.ifndef>
       <tapa.flag id="CZAC_LKSCTP"/>
      </tapa.ifndef>
      <tapa.mode mode="single"/>

   </tapa.require>

   # Configuration subsection
   <tapa.cfg>
      <tapa.hi.init/>
      <tapa.sb.init/>
      <tapa.cz.enb1.init/>
      <tapa.cz.dynamenb2.init dynamPeerFlag="TRUE"/>


   </tapa.cfg>

   # Test Steps subsection
   <tapa.test>

      # Send an assoc response from eNB2 for dynamic peer with Wrong SPID 
      <tapa.send>
         <tapa.czt.assocrsp procId="CZAC_LCZ_ENB2_PRC" peerId="1" 
                            spId="9" addressType="4" 
                            priDstAddr="127.0.0.1" 
                            dstPort="10002" comProcStrm="1" 
                            locOutStrms="5" status="CZT_PRIM_OK" 
                            cause="CZT_STACAUSE_NOT_APPL"/>
      </tapa.send>

             <tapa.expset>
         # Alarams are getting drop. Check it
                   <tapa.lcz.staInd peerId="2" category="LCM_CATEGORY_INTERFACE"
                          event = "LCZ_EVENT_UI_INV_EVT"
                          cause = "LCZ_CAUSE_INV_SPID"
                          type   =  "LCZ_USTA_DGNVAL_SAPID"
                          typeId =  "9"
                          procId="CZAC_LCZ_ENB2_PRC" />
        </tapa.expset>

     <tapa.delay delay="3" />
   
      # Send an assoc response from eNB2 for dynamic peer which Dynamic Peer Ind
      # is not received.
      <tapa.send>
         <tapa.czt.assocrsp procId="CZAC_LCZ_ENB2_PRC" peerId="1" 
                            spId="0" addressType="4" 
                            priDstAddr="127.0.0.1" 
                            dstPort="10002" comProcStrm="1" 
                            locOutStrms="5" status="CZT_PRIM_OK" 
                            cause="CZT_STACAUSE_NOT_APPL"/>
      </tapa.send>
     <tapa.delay delay="3" />
     <tapa.expset>
             <tapa.czt.staind peerId="1" staType="CZT_STA_TYPE_PEER"
                        procId="CZAC_LCZ_ENB2_PRC" 
                        status="CZT_STATUS_DYNAM_PEERCFG_NOK"
                        reason="CZT_REASON_NOT_APPL"/>
           </tapa.expset>

     
   </tapa.test>

   # Clean subsection
   <tapa.clean/>

   # Shutdown subsection
   <tapa.shutdown>

      # x2ap cleanup operations
       <tapa.cz.dynamenb2.deinit/>
       <tapa.cz.enb1.deinit/>
      <tapa.sb.deinit/>
      <tapa.hi.deinit/>

   </tapa.shutdown>

</tapa.testcase>


