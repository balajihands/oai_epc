TOP = $(shell pwd)/../..

PRODUCTAWARE := yes

TARGET_NAME = Radisys-mib-core

AUTOGENERATED_DIR := $(TOP)/libs/common/mib-common/obj/$(PRODUCT)/generic/generic

ifeq ($(PLTFRM),PAL)
CPPFLAGS = -g3 -Werror -m32
else
CPPFLAGS = -g3 -Werror
endif

INCLUDE_PATHS := \
  $(TOP)/libs \
  $(TOP)/libs/common/include \
  $(TOP)/libs/common \
  $(AUTOGENERATED_DIR)

ifeq ($(STATICALLY_LINKED),yes)
  include $(TOP)/makefrags/staticlibrary.mk
else
  include $(TOP)/makefrags/sharedlibrary.mk
endif
